\zknag_kompletuj
::----------------------------------------------------------------------------------------------------------------------
::  UTW: [rr] [17.00]
:: OPIS: Formuła Dołaczenia Kompletacji
::----------------------------------------------------------------------------------------------------------------------
:: NUCO - dodanie wywołania konkretnego procesu poprzez sygnał
{? ~exec('record_keyrefed','#b__box',ZK_N.uidref(),'LMO_MOB_DKZK')
|| _par:=obj_new(1);
   _par[1]:=obj_new('PARAMETR','VALUE');
   _par[1].PARAMETR:='ZK_N';
   _par[1].VALUE:=ZK_N.ref();
   exec('force_signal','#b__box','NUCO_ZKN_MOB',,_par)
|| _params:=exec('mp_run_a','#b__box');
   _params.ACT_UID:='LMO_MOB_DKZK';
   _params.UIDREF:=ZK_N.uidref();
   _params.AKCJA:='Do wysyłki';
   _params.PORTS_IN:=exec('portsIn','#b__box',_params.ACT_UID);
   exec('portsInSet','#b__box',_params.PORTS_IN,_params.ACT_UID,'ZK_N',ZK_N.ref());
   exec('mp_run','#b__box',_params)
?}


