      w[5]:=form({? ZK_P.N().CB='T' || ZK_P.CB || ZK_P.CN ?},,doklw,'.,');
      w[6]:=form(ZK_P.IL2*{? ZK_P.N().CB='T' || ZK_P.CB || ZK_P.CN ?},,doklw,'.,');
      w[7]:=ZK_P.J2().KOD;
      w[8]:=ZK_P.IL2-ZK_P.ILP;
      w[12]:=exec('stan','qlsp',ZK_P.M,DEFINE.DAT_REAL);
      w[8]:=exec('real2date','qlsp',DEFINE.DAT_REAL,ZK_P.POZ);
      w[9]:=ZK_P.ILP;
      w[9]:=ZK_P.IL2-w[8];
      w[10]:=ZK_P.MG().SYM;
      w[11]:=form(((w[8]+w[12])/ZK_P.IL2)*100,,doklw,'.,');
      s[1]+=(ZK_P.IL2*{? ZK_P.N().CB='T' || ZK_P.CB || ZK_P.CN ?})$doklw;

      _f:=prn1+'.ini_line()'; _v:=($(_f))();
