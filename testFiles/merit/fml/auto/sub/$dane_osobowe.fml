\dolacz_osobe
::----------------------------------------------------------------------------------------------------------------------
::   UTW: ARTSLO [19.22]
::  OPIS: test dodania OSOBY w Danych Osobowych
::  AREA: [ZWS_DOS]
:: TABLE: [OSOBA]
::    WE: _a - wskazanie na obiekt testowy
::        _b - dane do wpisania zgodne ze strukturą w \bf_os_add/$dane_osobowe.fml
::        _c - uruchomienie z pulpitu poprzez proces / z obszaru poprzez dołącz [1/0]
::        _d - jeśli dostepne są różne procesy akronim którym chcemy uruchomić
::    WY: [1/0] - powodzenie operacji/porażka lub wystąpienie błędu
::----------------------------------------------------------------------------------------------------------------------
::GIVEN
_test_driver:=_a;
_test_driver.logger.info('START WYWOŁANIE: \\dolacz_osobe/$dane_osobowe.fml (time: '+$date()+' '+(time()$3)+')');
_osoba:=_b;
_proces:=_c;
_pro_acr:=_d;
_wynik:=1;

::WHEN
{? ~_proces
|| _test_driver.click('btn:DOŁĄCZ');
   {? (_test_driver.exist('wnd:@tmp.aasdfjeoifjjeaf') &
      _test_driver.state('wnd:@tmp.aasdfjeoifjjeaf').title='Procesy') |
      _test_driver.exist('btn:@Uruchom')
   || _test_driver.filter('@tmp.UID', _pro_acr);
      _test_driver.click('fld:@tmp.UID(row=1)');
      _test_driver.assert('Brak procesu '+_pro_acr+'.', 'fld:@tmp.UID(row=1)', 'value', _pro_acr);
      _test_driver.click('btn:@Uruchom')
   ?}
?};
_test_driver.click('fld:@OSOBA.NAZWISKO');
_test_driver.send(_osoba.NAZWISKO,'','fld:@OSOBA.NAZWISKO');
_test_driver.click('fld:@OSOBA.RODOWE');
_test_driver.send(_osoba.RODOWE,'','fld:@OSOBA.RODOWE');
_test_driver.click('fld:@OSOBA.PIERWSZE');
_test_driver.send(_osoba.PIERWSZE,'','fld:@OSOBA.PIERWSZE');
_test_driver.click('fld:@OSOBA.DRUGIE');
_test_driver.send(_osoba.DRUGIE,'','fld:@OSOBA.DRUGIE');
_test_driver.click('fld:@OSOBA.PESEL');
_test_driver.send(_osoba.PESEL,'','fld:@OSOBA.PESEL');
{? _test_driver.exist('dial:') & _test_driver.state('dial:').message='Niepoprawny numer PESEL.\nBłąd weryfikacji sumy kontrolnej.\nZatwierdzić informację?'
|| _test_driver.click('dbtn:Tak')
?};
{? _osoba.PLEC='K'
|| _test_driver.click('fld:@OSOBA.PLEC(ndx=1)')
|| _test_driver.click('fld:@OSOBA.PLEC(ndx=2)')
?};
:: odpowiedź na pytanie czy już istatnieje taka osoba w bazie, przerywamy i zwracamy, że nie dodano bo już ktoś o takim
:: pesel jest w bazie
{? _test_driver.exist('dbtn:Nie')
|| _test_driver.click('dbtn:Nie');
   _wynik:=0;
   _test_driver.send('', '{ESC}', '')
|| _test_driver.click('fld:@OSOBA.UR_DATA');
   _test_driver.send(_osoba.UR_DATA,'','fld:@OSOBA.UR_DATA');
   _test_driver.click('fld:@OSOBA.UR_MIEJ');
   _test_driver.send(_osoba.UR_MIEJ,'','fld:@OSOBA.UR_MIEJ');
   _test_driver.click('fld:@OSOBA.OJCIECN');
   _test_driver.send(_osoba.OJCIECN,'','fld:@OSOBA.OJCIECN');
   _test_driver.click('fld:@OSOBA.OJCIEC');
   _test_driver.send(_osoba.OJCIEC,'','fld:@OSOBA.OJCIEC');
   _test_driver.click('fld:@OSOBA.MATKA');
   _test_driver.send(_osoba.MATKA,'','fld:@OSOBA.MATKA');
   _test_driver.click('fld:@OSOBA.OBCY');
   {? _osoba.OBCY='T'
   || _test_driver.send('checked', '', 'fld:@OSOBA.OBCY')
   || _test_driver.send('unchecked', '', 'fld:@OSOBA.OBCY')
   ?};

   _test_driver.click('fld:@OSOBA.KARTA');
   {? _osoba.KARTA='T'
   || _test_driver.send('checked', '', 'fld:@OSOBA.KARTA')
   ?};
:: nie wymagamy już podania narodowości
::_test_driver.click('fld:@OSOBA.NAROD');
::_test_driver.send(_osoba.NAROD,'','fld:@OSOBA.NAROD');
   {? _osoba.OBCY='T'
   || _test_driver.click('fld:@OSOBA.OBYWATEL');
      _test_driver.send(_osoba.OBYWATEL,'','fld:@OSOBA.OBYWATEL')
   ?};
   _test_driver.click('fld:@OSOBA.NIP');
   _test_driver.send(_osoba.NIP,'','fld:@OSOBA.NIP');
:: nie wymagamy już podania dowodu
::   _test_driver.click('fld:@OSOBA.DOWOD');
::   _test_driver.send(_osoba.DOWOD,'','fld:@OSOBA.DOWOD');
::   _test_driver.click('fld:@OSOBA.ORG_DOW');
::   _test_driver.send(_osoba.ORG_DOW,'','fld:@OSOBA.ORG_DOW');
::   _test_driver.click('fld:@OSOBA.DWY_DOW');
::   _test_driver.send(_osoba.DWY_DOW,'','fld:@OSOBA.DWY_DOW');
::   _test_driver.click('fld:@OSOBA.DWA_DOW');
::   _test_driver.send(_osoba.DWA_DOW,'','fld:@OSOBA.DWA_DOW');
   {? _osoba.KARTA='T'
   || _test_driver.click('fld:@OSOBA.KP');
      _test_driver.send(_osoba.KP,'','fld:@OSOBA.KP');
      _test_driver.click('fld:@OSOBA.ORG_KP');
      _test_driver.send(_osoba.ORG_KP,'','fld:@OSOBA.ORG_KP');
      _test_driver.click('fld:@OSOBA.DWY_KP');
      _test_driver.send(_osoba.DWY_KP,'','fld:@OSOBA.DWY_KP');
      _test_driver.click('fld:@OSOBA.DWA_KP');
      _test_driver.send(_osoba.DWA_KP,'','fld:@OSOBA.DWA_KP')
   ?};
   _test_driver.click('fld:@OSOBA.PASZPORT');
   _test_driver.send(_osoba.PASZPORT,'','fld:@OSOBA.PASZPORT');
   _test_driver.click('fld:@OSOBA.ORG_PASZ');
   _test_driver.send(_osoba.ORG_PASZ,'','fld:@OSOBA.ORG_PASZ');
   _test_driver.click('fld:@OSOBA.KRAJPASZ.NAZ');
   _test_driver.send(_osoba.NAZ,'','fld:@OSOBA.KRAJPASZ.NAZ');
   _test_driver.click('fld:@OSOBA.DWY_PASZ');
   _test_driver.send(_osoba.DWY_PASZ,'','fld:@OSOBA.DWY_PASZ');
   _test_driver.click('fld:@OSOBA.DWA_PASZ');
   _test_driver.send(_osoba.DWA_PASZ,'','fld:@OSOBA.DWA_PASZ');
   _test_driver.click('fld:@OSOBA.KWW_NAZ');
   _test_driver.send(_osoba.KWW_NAZ,'','fld:@OSOBA.KWW_NAZ');
   _test_driver.click('fld:@OSOBA.KWW_IM');
   _test_driver.send(_osoba.KWW_IM,'','fld:@OSOBA.KWW_IM');
   _test_driver.click('fld:@OSOBA.KWW_TEL');
   _test_driver.send(_osoba.KWW_TEL,'','fld:@OSOBA.KWW_TEL');
   _test_driver.click('fld:@OSOBA.KWW_MAIL');
   _test_driver.send(_osoba.KWW_MAIL,'','fld:@OSOBA.KWW_MAIL');
   {? _test_driver.state('btn:@Akceptuj').enable='true'
   || _test_driver.click('btn:@Akceptuj');
      {? _test_driver.exist('dbtn:Tak')
      || _test_driver.click('dbtn:Tak')
      ?}
   || _test_driver.click('btn:@Zapisz')
   ?}
?};
::THEN
::do przemyslenia jak określić powodzenie tego testu, na razie istnienie rekordu w tabeli OSOBA
{? _wynik=0
|| _test_driver.assert_false('Istnieje osoba o podanym pesel',_wynik=0)
|| _test_driver.click('wnd:zws_dos_osoba');
