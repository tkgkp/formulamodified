\init
::----------------------------------------------------------------------------------------------------------------------
::  UTW: jaws [18.42]
:: OPIS: Inicjalizuje mechanizm importu/eksportu.
::   WE: _a [OBJECT] - środowisko mechanizmu
::  TAG: <PUBLICZNA>
::----------------------------------------------------------------------------------------------------------------------
_def:=_a;

_def.ID:='UPR_ZAWK';
_def.FUNPAR:='ZWS_PAR_PUZW';
_def.DOMAIN:=exec('name','#b_domain','PKD');
_def.FILE:=exec('def_per_dict_xlsx','xls__init');
_def.SHEET:='Uprawnienia zawodowe=1,1';
_def.NAME:='Uprawnienia zawodowe'@;
_def.DESC:='Uprawnienia zawodowe';
_def.MULTIFIR:='T';

: źródło danych
_def.TABLE:="UPR_ZAWK";

: prolog
_def.BEFORE:="
   ADRES.cntx_psh();
   ADRES.prefix();
   UPR_ZAWK.cntx_psh();
   UPR_ZAWK.f_clear();
   UPR_ZAWK.index('SYMBOL');
   UPR_ZAWK.prefix();
   ~~
";

: epilog
_def.AFTER:="
   UPR_ZAWK.cntx_pop();
   ADRES.cntx_pop();
   ~~
";

: definicja kolumn
_def.FIELDS:="
   _env:=_a;
   _env.define('SYMBOL', MS.name(UPR_ZAWK,'SYMBOL'), 1,,MS.comment(UPR_ZAWK,'SYMBOL'));
   _env.define('NAZWA',  MS.name(UPR_ZAWK,'NAZWA'),  1,,MS.comment(UPR_ZAWK,'NAZWA'));
   _env.define('SLO_TYP',MS.name(UPR_ZAWK,'SLO_TYP'),1,,MS.comment(UPR_ZAWK,'SLO_TYP'));
   _env.define('ZAWIES', MS.name(UPR_ZAWK,'ZAWIES'), 1,,MS.comment(UPR_ZAWK,'ZAWIES'));
   _env.define('NR',     MS.name(UPR_ZAWK,'NR'),     1,,MS.comment(UPR_ZAWK,'NR'));
   _env.define('OD',     MS.name(UPR_ZAWK,'OD'),     1,,MS.comment(UPR_ZAWK,'OD'));
   _env.define('DO',     MS.name(UPR_ZAWK,'DO'),     1,,MS.comment(UPR_ZAWK,'DO'));
   _env.define('UTRATA', MS.name(UPR_ZAWK,'UTRATA'), 1,,MS.comment(UPR_ZAWK,'UTRATA'));
   _env.define('DOK_NR', MS.name(UPR_ZAWK,'DOK_NR'), 1,,MS.comment(UPR_ZAWK,'DOK_NR'));
   _env.define('DOK_DT', MS.name(UPR_ZAWK,'DOK_DT'), 1,,MS.comment(UPR_ZAWK,'DOK_DT'));
   _env.define('DOK_WYD',MS.name(UPR_ZAWK,'DOK_WYD'),1,,MS.comment(UPR_ZAWK,'DOK_WYD'));
   ~~
";

: zawartość wiersza
_def.EXPORT:="
   _env:=_a;
   _buf:=_b;
   _buf.SYMBOL.VALUE:=UPR_ZAWK.SYMBOL;
   _buf.NAZWA.VALUE:=UPR_ZAWK.NAZWA;
   _buf.SLO_TYP.VALUE:=UPR_ZAWK.SLO_TYP().SYMBOL;
   _buf.ZAWIES.VALUE:=UPR_ZAWK.ZAWIES;
   _buf.NR.VALUE:=UPR_ZAWK.NR;
   _buf.OD.VALUE:=UPR_ZAWK.OD;
   _buf.DO.VALUE:=UPR_ZAWK.DO;
   _buf.UTRATA.VALUE:=UPR_ZAWK.UTRATA;
   _buf.DOK_NR.VALUE:=UPR_ZAWK.DOK_NR;
   _buf.DOK_DT.VALUE:=UPR_ZAWK.DOK_DT;
   _buf.DOK_WYD.VALUE:=UPR_ZAWK.DOK_WYD;
   _env.write_async('SLO_TYP',UPR_ZAWK.SLO_TYP);
   1
";

: weryfikacja i zapis wiersza odczytanego z pliku wymiany
_def.VALIDATE:="exec('validate','xls_upr_zawk',_a,_b,_c)";
_def.IMPORT:="exec('import','xls_upr_zawk',_a,_b,_c)";
