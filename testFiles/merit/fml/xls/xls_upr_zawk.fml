\validate
::----------------------------------------------------------------------------------------------------------------------
::  UTW: jaws [18.42]
:: OPIS: Weryfikuje poprawność wiersza odczytanego z pliku wymiany.
::   WE: _a [OBJECT] - tablica nazwana z wartościami pól wiersza pliku wymiany
::       _b [INTEGER] - tryb pracy: 0 - zachowaj istniejące, 1 - nadpisywać istniejące
::       _c [OBJECT] - obiekt z resultem, patrz exec('args_valid','#excel_imex')
::   WY: ~~
::----------------------------------------------------------------------------------------------------------------------
: mapa argumentów
_buf:=_a;
_mod:=_b;
_val:=_c;

: wychwyć błędy krytyczne
_fld:=
   {? _buf.SYMBOL=''  || 'SYMBOL'
   |? _buf.NAZWA=''   || 'NAZWA'
   |? _buf.ZAWIES=''  || 'ZAWIES'
   |? _buf.NR=''      || 'NR'
   |? _buf.OD=''      || 'OD'
   |? _buf.DO=''      || 'DO'
   |? _buf.UTRATA=''  || 'UTRATA'
   |? _buf.DOK_NR=''  || 'DOK_NR'
   |? _buf.DOK_DT=''  || 'DOK_DT'
   |? _buf.DOK_WYD='' || 'DOK_WYD'
   || ''
   ?};
{? _fld<>''
:  katastrofa
|| _val.msg_empty(_fld);
   _val.RESULT:=0;
   return()
?};

: błędne wartości
{? ('RPN'*_buf.ZAWIES)=0
|| _val.msg_inset('ZAWIES','R','P','N');
   _val.RESULT:=0;
   return()

|? ('NOW'*_buf.NR)=0
|| _val.msg_inset('NR','N','O','W');
   _val.RESULT:=0;
   return()

|? ('NOW'*_buf.OD)=0
|| _val.msg_inset('OD','N','O','W');
   _val.RESULT:=0;
   return()

|? ('NOW'*_buf.DO)=0
|| _val.msg_inset('DO','N','O','W');
   _val.RESULT:=0;
   return()

|? ('NO'*_buf.UTRATA)=0
|| _val.msg_inset('UTRATA','N','O');
   _val.RESULT:=0;
   return()

|? ('NOW'*_buf.DOK_NR)=0
|| _val.msg_inset('DOK_NR','N','O','W');
   _val.RESULT:=0;
   return()

|? ('NOW'*_buf.DOK_DT)=0
|| _val.msg_inset('DOK_DT','N','O','W');
   _val.RESULT:=0;
   return()

|? ('NOW'*_buf.DOK_WYD)=0
|| _val.msg_inset('DOK_WYD','N','O','W');
   _val.RESULT:=0;
   return()

|? _buf.SLO_TYP<>'' & _buf.DOK_WYD='N'
|| _val.msg_inset('DOK_WYD','O','W');
   _val.RESULT:=0;
   return()
?};

: ostrzeżenia
_max:=MS.fld_len(UPR_ZAWK,'SYMBOL');
{? +_buf.SYMBOL>_max
|| _val.msg_length('SYMBOL',_max);
   _buf.SYMBOL:=_max+_buf.SYMBOL
?};
_max:=MS.fld_len(UPR_ZAWK,'NAZWA');
{? +_buf.NAZWA>_max
|| _val.msg_length('NAZWA',_max);
   _buf.NAZWA:=_max+_buf.NAZWA
?};
_max:=MS.fld_len(SLO_TYP,'SYMBOL');
{? +_buf.SLO_TYP>_max
|| _val.msg_length('SLO_TYP',_max);
   _buf.SLO_TYP:=_max+_buf.SLO_TYP
?};

: weryfikacja spójności referencyjnej
_typ:=null;
{? _buf.SLO_TYP<>''
|| {? (_typ:=exec('slo_typ','ext_slo',_buf.SLO_TYP))=null
   || _val.msg_norec('SLO_TYP');
      _val.RESULT:=0;
      return()
   ?}
?};

: ustalenie, rekordów według kluczy unikalnych
_rzS:={? UPR_ZAWK.find_tab(,'SYMBOL',,'=',_buf.SYMBOL) || UPR_ZAWK.ref() || null ?};
_rzN:={? UPR_ZAWK.find_tab(,'NAZWA',,'=',_buf.NAZWA) || UPR_ZAWK.ref() || null ?};

: optymalizacja importu i standaryzacja kodu
_obj:=_val.OBJ:=exec('write_obj','xls__util',_rzS,
   "'%1 - %2'[_a.SYMBOL,_a.NAZWA]",
   'SLO_TYP',_typ
);

{? _rzS=null
|| {? _rzN=null
   || _val.ACTION:='add'
   || _val.msg_unique(_obj.to_str(_buf));
      _val.RESULT:=0
   ?}
|| {? _rzN=null | _rzN=_rzS
   || _val.ACTION:='put'
   || _val.msg_unique(_obj.to_str(_buf));
      _val.RESULT:=0
   ?}
?};
~~


\import
::----------------------------------------------------------------------------------------------------------------------
::  UTW: jaws [18.42]
:: OPIS: Tworzy lub modyfikuje wiersze na podstawie wartości odczytanych z pliku wymiany.
::   WE: _a [OBJECT] - tablica nazwana z wartościami pól wiersza pliku wymiany
::       _b [INTEGER] - tryb pracy: 0 - zachowaj istniejące, 1 - nadpisywać istniejące
::       _c [OBJECT] - rezultat walidacji i importu, patrz exec('args_valid','#excel_imex')
::   WY: ~~
::----------------------------------------------------------------------------------------------------------------------
exec('write','xls__util',_a,_b,_c,UPR_ZAWK,"
   _a.SYMBOL:=_b.SYMBOL;
   _a.NAZWA:=_b.NAZWA;
   _a.SLO_TYP:=_c.SLO_TYP;
   _a.ZAWIES:=_b.ZAWIES;
   _a.NR:=_b.NR;
   _a.OD:=_b.OD;
   _a.DO:=_b.DO;
   _a.UTRATA:=_b.UTRATA;
   _a.DOK_NR:=_b.DOK_NR;
   _a.DOK_DT:=_b.DOK_DT;
   _a.DOK_WYD:=_b.DOK_WYD;
   ~~
");
