\init
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Markus [18.42]
:: OPIS: Inicjalizuje mechanizm importu/eksportu do Excel
::   WE: _a - obj_new - środowisko mechanizmu
::  TAG: <PUBLICZNA>
::----------------------------------------------------------------------------------------------------------------------
_def:=_a;
_def.ID:='ISTDEFI';
_def.DOMAIN:=exec('name','#b_domain','ZWS');
_def.FUNPAR:='ZWS_DEF_IST';
_def.MULTIFIR:='T';

_def.HIDDEN:='T';
_def.FILE:='intrastat.xlsx';
_def.SHEET:='Atrybuty Intrastat=1,1';
_def.NAME:='Atrybuty Intrastat'@;
_def.DESC:='Atrybuty Intrastat';
_def.TABLE:="exec('table','xls_istdefi')";
_def.FIELDS:="exec('fields','xls_istdefi',_a)";
_def.EXPORT:="exec('export','xls_istdefi',_a,_b)";
_def.IMPORT:="exec('import','xls_istdefi',_a,_b,_c)";
_def.VALIDATE:="exec('validate','xls_istdefi',_a,_b,_c)";
_def.PREFIX:="ISTDEFI.index('XLS'); ISTDEFI.prefix()";
_def.BEFORE:="ISTDEF.cntx_psh(); ISTDEFS.cntx_psh(); ISTDEFI.cntx_psh()";
_def.AFTER:="ISTDEF.cntx_pop(); ISTDEFS.cntx_pop(); ISTDEFI.cntx_pop()";
~~


