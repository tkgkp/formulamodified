\record
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Markus [18.42]
:: OPIS: Uzupełnia bufor tabeli wartościami obiektu
::   WE: _a - obj_new() - tablica nazwana z polami odczytanymi z excel
::       _b - INTEGER - tryb pracy: 0 - nie zastępować istniejących wartości, 1 - zastępować istniejące
::       _c - obiekt Result, zawiera .RESULT, .ACTION, .OBJ (własna tablica parametrów)
::----------------------------------------------------------------------------------------------------------------------
_obj:=_a;
_mode:=_b;
_validate:=_c;
ISTDEFI.prefix();
{? _validate.ACTION='add'
||
   ISTDEFI.blank();
   ISTDEFI.ISTDEFS:=_validate.OBJ.ISTDEFS
?};

ISTDEFI.LP:=_obj.LP;
ISTDEFI.NAZ:=_obj.NAZ;
ISTDEFI.NRFP:=_obj.NRFP;
ISTDEFI.WYM:=_obj.WYM;
ISTDEFI.TYP:=_obj.TYP;
ISTDEFI.SLOWNIK:=_obj.SLOWNIK;
ISTDEFI.NAZFLD:=_obj.NAZFLD;
ISTDEFI.REGULY:=_obj.REGULY;
ISTDEFI.OPIS_FLD:=_obj.OPIS_FLD;
~~


\export
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Markus [18.42]
:: OPIS: Wypełnia obiekt który zapisze w Excel wiersz danymi z rekordu
::       Użycie: export
::   WE: _a - obj_new - środowisko mechanizmu
::       _b - obj_new - obiekt który zasilić wartościami z pól
::   WY: 0 - porażka
::       1 - sukces
::----------------------------------------------------------------------------------------------------------------------
_excel:=_a;
_table:=_b;
_TABLE:=_excel.table();

_result:=ISTDEFI.ISTDEFS().ISTDEF().IST_ISTK='I';

{? _result
||
   _table.ISTDEFS.VALUE:=$ISTDEFI.ISTDEFS().LP;
::   _excel.write_async('ISTDEFS',ISTDEFI.ISTDEFS);

   _table.TM_STAMP.VALUE:=$ISTDEFS.ISTDEF().TM_STAM;
::   _excel.write_async('ISTDEF',ISTDEFS.ISTDEF);

   _table.LP.VALUE:=ISTDEFI.LP;
   _table.NAZ.VALUE:=ISTDEFI.NAZ;
   _table.NRFP.VALUE:=ISTDEFI.NRFP;
   _table.WYM.VALUE:=ISTDEFI.WYM;
   _table.TYP.VALUE:=ISTDEFI.TYP;
   _table.SLOWNIK.VALUE:=ISTDEFI.SLOWNIK;
   _table.NAZFLD.VALUE:=ISTDEFI.NAZFLD;
   _table.REGULY.VALUE:=ISTDEFI.REGULY;
   _table.OPIS_FLD.VALUE:=ISTDEFI.OPIS_FLD
?};

_result


\validate
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Markus [18.42]
:: OPIS: Waliduje czy można poprawić/dodać rekord
::   WE: _a - obj_new() - tablica nazwana z polami odczytanymi z excel
::       _b - INTEGER - tryb pracy: 0 - nie zastępować istniejących wartości, 1 - zastępować istniejące
::       _c - obiekt Result, zawiera .RESULT, .ACTION, .OBJ (własna tablica parametrów do przekazania dla import)
::   WY: 0 - porażka
::       1 - sukces
::  TAG: <PUBLICZNA>
::----------------------------------------------------------------------------------------------------------------------
_obj:=_a;
_mode:=_b;
_result:=_c;

_result.RESULT:=1;

_result.OBJ:=obj_new('ISTDEFS', 'ISTDEF');

ISTDEF.cntx_psh();
ISTDEF.index('LP');
ISTDEF.prefix('','','I',#_obj.TM_STAMP);
_result.OBJ.ISTDEF:={? ISTDEF.first() || ISTDEF.ref() || null() ?};
ISTDEF.cntx_pop();
{? _result.OBJ.ISTDEF=null()
||
   _result.MSG:='Nie znaleziono definicji Intrastat o znaczniku czasowym %1.'@[_obj.TM_STAMP];
   _result.RESULT:=0;
   return(~~)
?};

_result.OBJ.ISTDEFS:=exec('FindInSet','#table','ISTDEFS','LP',#_obj.ISTDEFS, _result.OBJ.ISTDEF,,,,null());
{? _result.OBJ.ISTDEFS=null()
||
   _result.MSG:='Nie znaleziono wersje deklaracji o lp %1.'@[_obj.ISTDEFS];
   _result.RESULT:=0;
   return(~~)
?};

{? _result.RESULT = 1
||
   ISTDEFI.index('LP');
   ISTDEFI.prefix(_result.OBJ.ISTDEFS, _obj.LP);
   {? ISTDEFI.first() & ISTDEFI.LP = _obj.LP
   ||
      {? _mode=0
      || _result.RESULT:=0
      ||
         _result.ACTION:='put';
         exec('record','xls_istdefi',_obj,_mode,_result)
::         {? exec('spr_ISTDEFI','ISTDEFI_slu')<>''
::         ||
::            _result.RESULT:=0
::         ?}
      ?}
   ||
      _result.ACTION:='add';
      exec('record','xls_istdefi',_obj,_mode,_result)
::      {? exec('spr_ISTDEFI','ISTDEFI_slu')<>''
::      || _result.RESULT:=0
::      ?}
   ?}
?};
_result


\import
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Markus [18.42]
:: OPIS: Tworzy lub modyfikuje rekord na podstawie wartości odczytanych z Excel
::       Użycie: import
::   WE: _a - obj_new() - tablica nazwana z polami odczytanymi z excel
::       _b - INTEGER - tryb pracy: 0 - nie zastępować istniejących wartości, 1 - zastępować istniejące
::       _c - obiekt Result, zawiera .RESULT, .ACTION, .OBJ (własna tablica parametrów)
::   WY: 0 - porażka
::       1 - sukces
::----------------------------------------------------------------------------------------------------------------------
_obj:=_a;
_mode:=_b;
_validate:=_c;

_result:=0;
{? _validate.RESULT=1
||
   {? _validate.ACTION='add'
   ||
      _result:=ISTDEFI.add(1)
   |? _validate.ACTION='put'
   ||
      _result:=ISTDEFI.put(1)
   ?}
?};
{? _result=0
||
   {? _validate.ACTION='add'
   ||
      FUN.info('Dodanie atrybutu %1 struktury %2 definicji Intrastat %3 nie powiodło się.'@[$_obj.NAZ,_obj.ISTDEFS,_obj.TM_STAMP])
   ||
      FUN.info('Poprawa atrybutu %1 struktury %2 definicji Intrastat %3 nie powiodła się.'@[$_obj.NAZ,_obj.ISTDEFS,_obj.TM_STAMP])
   ?}
?};
_result

