wart_par:='';
{!|?
   {? REF.WFIRM=0
   || exec('load_par','#parametr',1);
      PARAMETR.hdr_sel();
      PARAMETR.hdr_sel('Parametry systemu - WERSJA JEDNOFIRMOWA'@)
   |? PARADM.TRYB=1
   || exec('load_par','#parametr',1);
      PARAMETR.hdr_sel();
      PARAMETR.hdr_sel('Parametry wzorcowe systemu - TRYB ADMINISTRATORA'@)
   || exec('load_par','#parametr');
      PARAMETR.hdr_sel();
      PARAMETR.hdr_sel('Parametry systemu - %1'@[REF.S_FIRMA().OPIS])
   ?};
   PARAMETR.index(PARNUMER);
   PARAMETR.prefix();
   {? ~PARAMETR.find_tab(1,'NUMER',,'=',PARADM.LAST) || PARAMETR.first() ?};
   PARAMETR.select(,1,30)
!};
PARADM.TRYB:=0;
PARADM.LAST:=0;
exec('load_par','#parametr');
