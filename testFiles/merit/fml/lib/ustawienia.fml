\findkraj
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Mario [2006]
:: OPIS: szukanie waluty podanego kraju
::   WE: _a - kod kraju np. 'GB'
::       [_b - 0,1,2]
::   WY: w zalezności od parametru _b jest to wskazanie na
::       kraj (_b nie przekazane) KRAJE.ref
::       walute (_b=0) SLO.ref
::       slownik stawek VAT (_b=1) SLU.ref
::       kraj (_b=2) w słowniku krajów SLO.ref
::  OLD: \findkraj/faktury1.fml
::----------------------------------------------------------------------------------------------------------------------
SLO.cntx_psh();
SLU.cntx_psh();
KRAJE.cntx_psh();
{? _
|| KRAJE.index('KRAJE');
   KRAJE.prefix();
   _wy:={? KRAJE.find_key(_a) || KRAJE.ref || null ?}
?};
{? _=2
|| _wy:={? _b=0
        || KRAJE.WAL
        |? _b=1
        || SLU.index('NAZ');
           SLU.prefix();
           {? SLU.find_key('~STAWKI VAT '+_a)
           || SLU.ref
           || null
           ?}
        |? _b=2
        || SLU.index('NAZ');
           SLU.prefix();
           {? SLU.find_key('~KRAJE UE')
           || SLO.index('SL');
              SLO.prefix(SLU.ref);
              {? SLO.find_key(_a)
              || SLO.ref
              || null
              ?}
           || null
           ?}
        || null
        ?}
?};
KRAJE.cntx_pop();
SLU.cntx_pop();
SLO.cntx_pop();
_wy


\wz_sv
::----------------------------------------------------------------------------------------------------------------------
::  UTW: Mario [2008]
:: OPIS: wzorzec dla slownika stawek vat
::   WE: czy brac pod uwage tylko zapis w tabeli info T/N - domyslnie N
::  OLD: \wz_sv/tr_slown.fml
::----------------------------------------------------------------------------------------------------------------------
{? _>=1 || {? type_of(_a)<>2 || _a:='N' ?} || _a:='N' ?};
{? _a='N' & 5+cur_tab.name()='fakpo' & KSTE.KRAJ<>null
||
:: slownik wspolnoty
   {? (_a:=exec('findkraj','ustawienia',KSTE.KRAJ().KOD,1))
   || SLU.prefix();
      {? SLU.seek(_a)
      || SLU.get()
      ?}
   ?}
|? _a='N' & KSTE.KRAJ<>null
||
   SLU.index('NAZ');
   SLO.index('SL');
   SLU.prefix();
   {? SLU.find_key('~STAWKI VAT '+KSTE.KRAJ().KOD)
   || SLU.get()
   ?}
|| SLU.index('NAZ');
   SLO.index('SL');
   SLU.prefix();
   {? SLU.find_key('~STAWKI VAT '+INFO.KRAJ().KOD)
   || SLU.get()
   ?}
?};
''


\jakislsv
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MZ [2008]
:: OPIS: slownik stawek vat
::   WE: [_a]- 1 - nie wywoluje cur_tab  2 - dziala dla KSTE.KRAJ
::   WY: '' - czy znaleziono slownik stawek VAT 'brak'-nie znaleziono
::  OLD: \jakislsv/defin.fml
::       \wz_sv/tr_slown.fml
::----------------------------------------------------------------------------------------------------------------------
_wyn:='';
_get:=0;
{? _>=1  || {? type_of(_a)<>1 || _a:=0 ?} || _a:=0 ?};

exec('czytaj','#stalesys',,INFO,'KRAJ');

_kraj_vat:={? BEER.KRAJ_VAT || BEER.KRAJ_VAT().SYM || KSTE.KRAJ().KOD ?};

_poz:=_a=0 & ('fakpo zkpoz'*(5+cur_tab.name()) | 'dokma'*(5+cur_tab.name()) & DK.PLUS='N' & ND.TYP().Z='T');

{? _a=0 & _poz & _kraj_vat<>''
||
:: slownik wspolnoty
   {? (_a:=exec('findkraj','ustawienia',_kraj_vat,1))
   || SLU.prefix();
      _wyn:={? SLU.seek(_a) || _get:=1; '' || 'brak' ?};
      SLU.get()
   ?}
|? _a=2
||
   {? _kraj_vat<>''
   || SLU.index('NAZ');
      SLO.index('SL');
      SLU.prefix();
      {? SLU.find_key('~STAWKI VAT '+_kraj_vat)
      || SLU.get();
         SLO.prefix(SLU.ref);
         _get:=1;
         _wyn:=''
      || FUN.info('Brak słownika: ~STAWKI VAT %1.'@[_kraj_vat])
      ?}
   || FUN.info('Niewypełniony kraj w stałych oddziału.\nNie można określić słownika dla Stawek VAT.'@)
   ?}
|| {? INFO.KRAJ<>null()
      & (exec('on','#mwapi') | (_a=0 & cur_tab(1)=M) | (INFO.KRAJ=KSTE.KRAJ) | (_a=0 & KSTE.KRAJ=null))
   || SLU.index('NAZ');
      SLO.index('SL');
      SLU.prefix();
      {? SLU.find_key('~STAWKI VAT '+INFO.KRAJ().KOD)
      || SLU.get();
         SLO.prefix(SLU.ref);
         _get:=1;
         _wyn:=''
      || FUN.info('Brak słownika: ~STAWKI VAT %1.'@[INFO.KRAJ().KOD])
      ?}
   |? INFO.KRAJ<>KSTE.KRAJ & KSTE.KRAJ<>null
   || SLU.index('NAZ');
      SLO.index('SL');
      SLU.prefix();
      {? SLU.find_key('~STAWKI VAT '+KSTE.KRAJ().KOD)
      || SLU.get();
         SLO.prefix(SLU.ref);
         _get:=1;
         _wyn:=''
      || FUN.info('Brak słownika: ~STAWKI VAT %1.'@[KSTE.KRAJ().KOD])
      ?}
   || FUN.info('Niewypełniony kraj w stałych systemu.\nNie można określić słownika dla Stawek VAT.'@)
   ?}
?};
{? _get || SLO.hdr_sel(''); SLO.hdr_sel(SLU.OP) ?};
''


