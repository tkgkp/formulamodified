\odd_par_1
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AMK [17.28]
:: OPIS: Zwraca jednostkę księgową z pierwszego parametru systemu lub CENTRALĘ
::   WE: _a - opcja - ref rekordu w tabeli FIRMA
::   WY: ODD.ref()
::----------------------------------------------------------------------------------------------------------------------
_firma:={? _ & type_of(_a)=type_of(null) & _a
        || _a
        || exec('ref_firma','ustawienia')
        ?};
exec('odd_new','ustawienia',_firma);
_zwrot:=null;
_odd:=PAR_SKID.get(1);
{? _odd='' || _odd:='CENTRALA' ?};
ODD.cntx_psh(); ODD.index('ODDZIALY'); ODD.prefix(_firma,_odd,);
{? ODD.first() || _zwrot:=ODD.ref() ?};
ODD.cntx_pop();
{? ~_zwrot
|| ODD.cntx_psh(); ODD.index('ODDZIALY'); ODD.prefix(_firma,'CENTRALA',);
   {? ODD.first() || _zwrot:=ODD.ref() ?};
   ODD.cntx_pop()
?};
