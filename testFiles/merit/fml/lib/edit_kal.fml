\init
::----------------------------------------------------------------------------------------------------------------------
::  UTW: DRO [17.00]
:: OPIS:Inicjowanie tablic do edycji kalendarza
::   WE:
::   WY:
::----------------------------------------------------------------------------------------------------------------------
VAR_DEL.delete('par','tab');
par:=obj_new(10);
par[1]:=0;
par[2]:=date()~1;

kolor:="
      {? (_a %* 4)=0 || '0:0:0,255:128:0'
      |? (_a %* 4)=1 || '0:0:0,0:255:0'
      |? (_a %* 4)=2 || '0:0:0,255:255:0'
      || '0:0:0,128:255:255' ?}
       ";
opis:="
   {? _a=1 || 'styczeń'@
   |? _a=2 || 'luty'@
   |? _a=3 || 'marzec'@
   |? _a=4 || 'kwiecień'@
   |? _a=5 || 'maj'@
   |? _a=6 || 'czerwiec'@
   |? _a=7 || 'lipiec'@
   |? _a=8 || 'sierpień'@
   |? _a=9 || 'wrzesień'@
   |? _a=10|| 'październik'@
   |? _a=11|| 'listopad'@
   |? _a=12|| 'grudzień'@
   ?}
   ";
week:="
   {? _a=1 || 'poniedziałek'@
   |? _a=2 || 'wtorek'@
   |? _a=3 || 'środa'@
   |? _a=4 || 'czwartek'@
   |? _a=5 || 'piątek'@
   |? _a=6 || 'sobota'@
   |? _a=7 || 'niedziela'@
   ?}
   ";
week_short:="
   {? _a=1 || 'Pn'@
   |? _a=2 || 'Wt'@
   |? _a=3 || 'Śr'@
   |? _a=4 || 'Cz'@
   |? _a=5 || 'Pt'@
   |? _a=6 || 'So'@
   |? _a=7 || 'Nd'@
   || ''
   ?}
   "

