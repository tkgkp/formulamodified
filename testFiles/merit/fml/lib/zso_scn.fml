\akc_utw_dok
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [20.14]
:: OPIS: Akcja tworzenia dokumentu księgowego z OCR
::----------------------------------------------------------------------------------------------------------------------
_grp:=var_pres('GrSD')>0;
{? SKAN_DOK.STATUS='Z'
|| {? _grp
   || GrSD.JEST+=1
   || FUN.info('Dokument jest już zadekretowany.')
   ?}
|? SKAN_DOK.STATUS<>'D'
|| {? _grp
   || 1
   || FUN.info('Status uniemożliwia utworzenie dokumentu.')
   ?}
|? _grp
|| {? -SKAN_DOK.TYP='d' | SKAN_DOK.TYP=''
   || _ref:=SKAN_DOK.ref();
      _uid:=SKAN_DOK.uidref();
      _dok:=exec('add_dok','skanuj',1);
      {? _dok<>null
      || _params:=exec('mp_run_a','#b__box');
         _params.ACT_UID:='ZSO_SCN_TDOK';
         _params.AKCJA:='Grupowa';
         _params.UIDREF:=_uid;
         _params.PORTS_IN:=exec('portsIn','#b__box',_params.ACT_UID);
         exec('portsInSet','#b__box',_params.PORTS_IN,_params.ACT_UID,'SKAN_DOK',_ref);
         _params.PROC_START:='N';
         exec('mp_run','#b__box',_params);
         GrSD.OK+=1
      ?}
   || GrSD.INNY+=1
   ?}
|? FUN.ask('Czy utworzyć dokument księgowy?')
|| _ref:=SKAN_DOK.ref();
   SKAN_DOK.cntx_psh();
   _next:={? SKAN_DOK.next() | SKAN_DOK.prev() || SKAN_DOK.ref() || null ?};
   SKAN_DOK.cntx_pop();
   _params:=exec('mp_run_a','#b__box');
   _params.ACT_UID:='ZSO_SCN_TDOK';
   _params.AKCJA:='Dokument księgowy';
   _params.UIDREF:=SKAN_DOK.uidref();
   _params.PORTS_IN:=exec('portsIn','#b__box',_params.ACT_UID);
   exec('portsInSet','#b__box',_params.PORTS_IN,_params.ACT_UID,'SKAN_DOK',SKAN_DOK.ref());
   _params.PROC_START:='N';
   exec('mp_run','#b__box',_params);
   {? ~SKAN_DOK.seek(_ref) & _next<>null
   || SKAN_DOK.seek(_next)
   ?}
?}


