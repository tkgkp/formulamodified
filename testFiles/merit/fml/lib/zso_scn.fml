\akc_get
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [20.14]
:: OPIS: Pobranie danych dokumentu z OCR
::   WE: [_a] - zakończyć proces? [1]-tak 0-nie
::       [_b] - czy pytać o pobranie danych dokumentu: [1]-tak 0-nie
::----------------------------------------------------------------------------------------------------------------------
_grp:=var_pres('GrSD')>0;
_end:={? var_pres('_a')>0 || _a || 1 ?};
_ask:={? var_pres('_b')>0 || _b || 1 ?};
{? SKAN_DOK.STATUS='R'
|| {? _grp
   || GrSD.NOT+=1
   || FUN.info('Dokument nie był wysyłany.')
   ?}
|? SKAN_DOK.STATUS='S'
|| {? _grp
   || GrSD.NOTVER+=1
   || FUN.info('Dokument nie był zweryfikowany.')
   ?}
|? 'VG'*SKAN_DOK.STATUS=0
|| {? _grp
   || GrSD.JEST+=1
   || FUN.info('Dokument jest już pobrany.')
   ?}
|? exec('correct','skanuj') & (_grp | ~_ask | FUN.ask('Czy pobrać dane dokumentu?'))
|| _obj:=exec('get_doc','skanuj');
   {? _obj.file<>''
   || {? _obj.state=2
      || SKAN_DOK.STATUS:='D'
      ?};
      {? SKAN_DOK.bl_put('PLIK_OCR',_obj.file,1)
      || SKAN_DOK.TODO:='D';
         exec('set_nag','skanuj');
         {? SKAN_DOK.put()
         || ferase(_obj.file,1);
            {? exec('del_doc','skanuj')
            || SKAN_DOK.TODO:='';
               SKAN_DOK.put()
            ?};
            {? _end
            || _params:=exec('mp_run_a','#b__box');
               _params.ACT_UID:='ZSO_SCN_OCRD';
               _params.AKCJA:='Zakończ';
               _params.UIDREF:=SKAN_DOK.uidref();
               _params.PORTS_IN:=exec('portsIn','#b__box',_params.ACT_UID);
               exec('portsInSet','#b__box',_params.PORTS_IN,_params.ACT_UID,'SKAN_DOK',SKAN_DOK.ref());
               _params.PROC_START:='N';
               exec('mp_run','#b__box',_params)
            ?};
            {? _grp
            || GrSD.OK+=1
            ?}
         ?}
      ?}
   |? ~_grp
   || {? _obj.msg<>''
      || FUN.info('Informacja ze skanuj.to:\n%1'@[_obj.msg])
      || FUN.info('Nie udało się pobranie danych dokumentu od skanuj.to.')
      ?}
   ?}
?};
~~

