\akc_del
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [20.14]
:: OPIS: Usunięcie zeskanowanego dokumentu
::----------------------------------------------------------------------------------------------------------------------
_grp:=SKAN_DOK.sel_size();
{? _grp
|| _del:=1
|| _del:=0;
   {? SKAN_DOK.STATUS='R'
   || _del:=FUN.ask('Usunąć zeskanowany dokument?'@)
   || _del:=FUN.ask('Usunąć zeskanowany dokument?'@,'EXCLAM')
   ?}
?};
{? _del
|| {? 'DZR'*SKAN_DOK.STATUS=0
   || exec('del_doc','skanuj')
   ?};
   {? SKAN_DOK.STATUS='Z'
   || {? -SKAN_DOK.TYP='d' & +SKAN_DOK.OKRES=4
      || DOK.cntx_psh();
         DOK.use('doku'+SKAN_DOK.OKRES);
         DOK.index('DOKZRODL'); DOK.prefix();
         {? DOK.find_key($SKAN_DOK.ref(),)
         || DOK.DOKZRODL:='';
            DOK.put()
         ?};
         DOK.cntx_pop()
      |? -SKAN_DOK.TYP='f' & +SKAN_DOK.OKRES=2
      || EDOKUM.cntx_psh();
         EDOKUM.use('skid_v'+SKAN_DOK.OKRES);
         EDOKUM.index('DOK_POW'); EDOKUM.prefix();
         {? EDOKUM.find_key(SKAN_DOK.uidref(),)
         || EDOKUM.DOK_POW:='';
            EDOKUM.put()
         ?};
         EDOKUM.cntx_pop()
      ?}
   ?};
   DOKUM.cntx_psh();
   DOKUM.index('TYP'); DOKUM.prefix($SKAN_DOK.ref(),'I');
   {? DOKUM.first() || {! |? _uid:=DOKUM.uidref(); _rr:=DOKUM.del(); exec('dokumlog_del','zbl_dok',_uid); _rr !} ?};
   DOKUM.cntx_pop();
   SKAN_DOK.del();
   {? _grp || GrDel.DEL+=1 ?}
?}


