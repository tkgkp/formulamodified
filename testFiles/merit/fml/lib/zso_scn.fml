\akc_save
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [20.14]
:: OPIS: Akcja zapisz dla zeskanowanego dokumentu
::----------------------------------------------------------------------------------------------------------------------
_lp:=0;
{? SKAN_DOK.PLIK || _lp+=1 ?};
{? SKAN_DOK.PLIK_OCR || _lp+=2 ?};
_fld:='';
{? _lp=3
|| _lp:=FUN.choice('Który plik zapisać?',1,'Dokument','Dane')
?};
{? _lp=1
|| _fld:='PLIK'
|? _lp=2
|| _fld:='PLIK_OCR'
?};
{? _fld<>''
|| exec('bl_save','#blob',SKAN_DOK,_fld)
?};
''


\akc_send
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [20.14]
:: OPIS: Wysłanie dokumentu do OCR
::   WE: [_a] - komunikaty: [1]-tak 0-nie
:: ~OST: INBLPUT
::----------------------------------------------------------------------------------------------------------------------
{? 0
|| SKAN_DOK.STATUS:='D';
   SKAN_DOK.ID:='1';
   SKAN_DOK.bl_put('PLIK_OCR','@c:/praca/tmp/skanuj.to/003.bin',0,,'003.bin.json');
   exec('set_nag','skanuj');
   SKAN_DOK.put();
   _params:=exec('mp_run_a','#b__box');
   _params.ACT_UID:='ZSO_SCN_OCRD';
   _params.AKCJA:='Zakończ';
   _params.UIDREF:=SKAN_DOK.uidref();
   _params.PORTS_IN:=exec('portsIn','#b__box',_params.ACT_UID);
   exec('portsInSet','#b__box',_params.PORTS_IN,_params.ACT_UID,'SKAN_DOK',SKAN_DOK.ref());
   _params.PROC_START:='N';
   exec('mp_run','#b__box',_params);
   return(~~)
?};

_kom:={? var_pres('_a')>0 || _a || 1 ?};
_grp:=var_pres('GrSend')>0 & SKAN_DOK.sel_size();
{? SKAN_DOK.STATUS<>'R'
|| {? _grp
   || GrSend.JEST+=1
   |? _kom
   || FUN.info('Dokument jest już wysyłany.')
   ?}
|? exec('correct','skanuj') & (_kom=0 | _grp | FUN.ask('Czy wysłać dokument?'@))
|| _obj:=exec('send_doc','skanuj');
   {? _obj.id<>-1
   || SKAN_DOK.STATUS:={? _obj.state=5 || 'P' || 'S' ?};
      SKAN_DOK.ID:=$_obj.id;
      SKAN_DOK.put();
      {? _grp
      || GrSend.OK+=1
      ?}
   |? ~_grp
   || {? _obj.msg<>''
      || {? _kom
         || FUN.info('Informacja ze skanuj.to:\n%1'@[_obj.msg])
         ?}
      |? _kom
      || FUN.info('Nie udało się wysłanie dokumentu do skanuj.to.'@)
      ?}
   ?}
?};
