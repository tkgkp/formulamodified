_cfg.bipulp_wnds.kryt_tab:='Kryteria dostępu'@;
_cfg.bipulp_wnds.odb_ws:='WER';
_cfg.bipulp_wnds.odb_tab:='Odbiorcy'@;

_cfg.bipodb_usun.ask1:='Czy usunąć zaznaczonego odbiorcę?'@;
