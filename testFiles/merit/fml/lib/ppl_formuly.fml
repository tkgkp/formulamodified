::   WE:
::   WY: kwota składnika listy płac
::----------------------------------------------------------------------------------------------------------------------
O.T();
H.cntx_psh();
_RetVal:=1;
H.index('_HISTKOD');
H.prefix(P.ref,'Z');
{? H.find_le(date(O.R,O.M,0))
|| {? H.CZYWAL='T' | H.CZYWAL2='T' | H.CZYWAL3='T'
   || _RetVal:=exec('waluta','lista_licz',1,,,H.S1)
   ?}
?};
{? ~_RetVal
|| FUNKCJE.buff.error:=1;
   H.cntx_pop();
   return(0)
?};
H.cntx_pop;
DoList.d[1]:=date(O.R,O.M,1);
DoList.d[2]:=date(O.R,O.M,0);
exec('kal_prac','personel',O.R);
FUNKCJE.NR(1)


