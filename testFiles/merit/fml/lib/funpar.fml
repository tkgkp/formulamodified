   |? _tab.prefix(FO_DOM.B_DOMAIN().SYMBOL);
      {? _tab.size()=0
      || _tab.blank();
         _tab.B_DOMAIN:=B_DOMAIN.SYMBOL;
         _tab.add()
      ?};
      FO_DOM.next()
   !}
