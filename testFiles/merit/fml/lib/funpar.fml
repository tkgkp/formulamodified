_sql:='select distinct KST_URL.ROK from KST_URL order by 1 desc';
_sql+=' union distinct ';
_sql+='select distinct KART_URL.ROK '+
      'from KART_URL join P using (KART_URL.P,P.REFERENCE) '+
      'where P.FIRMA=:_a '+
      'order by 1 desc';

KST_DEF.cntx_psh();
KST_DEF.index('SYMBOL');
{! _lp:=1..3
|! _zm:={? _lp=1 || 'KST_PAR.URL_LIM' |? _lp=2 || 'KST.LIM_NSP' || 'KST.LIM_NAUK' ?};
   KST_DEF.prefix(_zm,);
   {? KST_DEF.first()
   || _sql+=' union distinct ';
      _sql+='select distinct extract(year from KST_WAR.DATA) ROK ';
      _sql+='from KST_WAR ';
      _sql+='where KST_WAR.KST_DEF=\''+$KST_DEF.ref()+'\' ';
      {? KST_DEF.WSPOLNA<>'T'
      || _sql+=' and KST_WAR.FIRMA=\''+$REF.FIRMA+'\' '
      ?};
      _sql+='order by 1 desc'
   ?}
!};
KST_DEF.cntx_pop();

