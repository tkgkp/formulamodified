: Uzupełnienie parametryzacji o nowe firmy
FP_DEF.cntx_psh;
FP_DEF.clear;
FP_DEF.index('ID');
_loop:=FP_DEF.first;
{!
|? _loop
|! exec('fp_def_fir','#funpar_str');
   _loop:=FP_DEF.next
!};
FP_DEF.cntx_pop;

