::----------------------------------------------------------------------------------------------------------------------
::  UTW: jaws [17.00]
:: OPIS: Wyświetla konsolę ustawień i parametryzacji
::----------------------------------------------------------------------------------------------------------------------
exec('__WND','#object');
: deklaruj niezbędne klasy
exec('dekl','#funpar_wid');
: utwórz obiekt pomocniczy
_pom:=obj_new(@.CLASS.FP_POM);
_pom.form_chk();

: obsługa startu zerowego
FP_STR.cntx_psh();
FP_STR.clear();
{? FP_STR.size()=0
|| exec('aktualizuj','funpar')
?};
FP_STR.cntx_pop();

: utwórz i wyświetl konsolę ustawień i parametryzacji
obj_new(@.CLASS.FP_KON,'__FP_KON',_pom).show();
obj_del(_pom);
~~


\aktualizuj
::----------------------------------------------------------------------------------------------------------------------
::  UTW: jaws [17.00]
:: OPIS: Aktualizacja zawartości parametryzatora
::   WE: [_a] - 0-bez progress-a 1(domyślnie)-z
::   WY:
::----------------------------------------------------------------------------------------------------------------------
_prog:={? var_pres('_a')=type_of(0) || _a || 1 ?};
FP_STR.cntx_psh();
{? _prog
|| progress(,'Proszę czekać. Trwa uzupełnianie konfiguracji...'@,FUN.TYT,1)
?};

: utwórz obiekt pomocniczy
exec('dekl_fp_pom','#funpar_str');
_pom:=obj_new(@.CLASS.FP_POM);
: aktualizuj konfigurację
exec('dane','funpar',_pom);
: uzupełnienie
_pom.batch();

{? _prog || prgs_clr() ?};
FP_STR.cntx_pop();

_pom.UPGD_ERR=0


\usun_czyn
::----------------------------------------------------------------------------------------------------------------------
::  UTW: GZ [18.42]
:: OPIS: Usuwa z drzewa parametryzacji czynność o podanym akronimie
::   WE: _a [STRING] - kod czynności
::----------------------------------------------------------------------------------------------------------------------
{? var_pres('_a')<>type_of('') || return ?};
FP_DEF.cntx_psh();
FP_STR.cntx_psh();
FP_DEF.index('ID');
FP_STR.index('FP_DEF');
FP_DEF.prefix('T',_a);
{? FP_DEF.first()
|| {!
   |? {? FP_STR.first()
      || {!
         |? FP_STR.del()
         !}
      ?};
      FP_DEF.next()
   !}
?};
FP_DEF.cntx_pop();
FP_STR.cntx_pop();
~~


