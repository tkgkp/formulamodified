      _last:=1;
      {! _i:=obj_len(TabNag.LISTA)//-1..1
      |? _ok
      |! {? TabNag.LISTA[_i].TYP=_a
         || _next:=TabNag.LISTA[_i].TAB.next();
            {? _next
            || _ok:=0;
               {? _a='K'
               || .ccol+=1
               || .crow+=1
               ?};
               {? TabNag.SEEKON || TabNag.seek(_i) ?}
            || {? TabNag.LISTA[_i].TAB.first() & TabNag.SEEKON
               || TabNag.seek(_i)
               ?};
               {? _last
               || _last:=0;
                  {? _a='K'
                  || .col:=.ccol
                  || .row:=.crow
                  ?}
               ?}
            ?}
         ?}
      !};
      _next
   ";
   TabNag.name:="
