                             ?};
::      TabNag.LISTA[_lp].TAB.win_sel(TabNag.LISTA[_lp].TAB.mk_sel(,,1));
::      TabNag.LISTA[_lp].TAB.select();
      K_P_VIEW.cntx_pop();
      _lp+=1;
      K_P_VIEW.next()
   !}
?};
