         _wyn:='';
         {!
         |? _p:=_fml*'[';
            {? _p
            || _wyn+=(_p-1)+_fml;
               _fml:=_p-_fml;
               _p:=_fml*']';
               _nr:=(_p-1)+_fml;
               {? 1+_nr='B' || _nr:=1-_nr ?};
               _fml:=_p-_fml;
               _wyn+={? _a='K'
                     || GSheet.cellName(.crow+.srow-1,.col+#_nr+.scol)
                     || GSheet.cellName(.row+#_nr+.srow-1,.ccol+.scol)
                     ?};
               1
            || _wyn+=_fml;
               0
            ?}
         !};
         _fml:='='+_wyn
      ?};
      _fml
   ";
   TabNag.isFormula:="
      _jest:=0;
      {! _i:=1..obj_len(TabNag.LISTA)
      |? _jest=0
      |! _jest:=TabNag.LISTA[_i].TAB.FORMULA1<>''
