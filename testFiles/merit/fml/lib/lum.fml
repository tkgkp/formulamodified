   _fb:="exec('set_efld_opt','umowy','INFO')";
   UM.grp_edit(_win_grp,UM,'INFO',,,_fb,,,'maximized');
   {? _umm
   ||
::    UMOWY MIEJSKIE
      _far:="exec('aftref_umm','lum')"; _fb:="exec('before_umm','lum')"; _fa:="exec('after_umm','lum')";
