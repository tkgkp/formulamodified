   UM.win_fml('WER_KHP',,'SYM',,'ICON_BEFORE',exec('um_sym_ib','umowy'));

   exec('init','lum');

:: Ustalenie treści linku
   _params:=params_get();
   {? type_of(_params)>0 & var_pres('LINK',_params)=type_of('')
   || _link:=_params.LINK
   || _link:=''
   ?};
   _oddz:='';
   _aneksowana:='';

:: Obsługa linku
   {? _link<>''
::    Ustalenie parametrów na podstawie przekazanego rekordu
   || _dobrze:=1;
     {? ref_tab(_link)=UM
     || _oddz:=exec('FindAndGet','#table',UM,_link,,"ODDZ",'');
        _aneksowana:=exec('FindAndGet','#table',UM,_link,,"A",'')
     |? ref_tab(_link)=UP
     || _oddz:=exec('FindAndGet','#table',UP,_link,,"exec('FindAndGet','#table',@.UM,UM,,\"ODDZ\",'')",'');
        _aneksowana:=exec('FindAndGet','#table',UP,_link,,"exec('FindAndGet','#table',@.UM,UM,,\"A\",'')",'')
     ?};
     {? _oddz=''
     || _dobrze:=0;
        {? ref_tab(_link)=UP
        || FUN.info('Nie odnaleziono pozycji umowy cyklicznej: %1'@[exec('record','#to_string',_link)])
        || FUN.info('Nie odnaleziono umowy cyklicznej: %1'@[exec('record','#to_string',_link)])
        ?}
     ?};
     {? _dobrze
     || _oddz_ref:=exec('FindInSet','#table','ODDZ','KOD2',_oddz,,,1);
::   Wykluczenie dostępu na podstawie uprawnień do danych - oddziały
        {? ~exec('spr_upr','users','ODDZ',_oddz_ref)
        || _dobrze:=0
        ?}
     ?};
:: Pokazują się tylko umowy nieaneksowane
     {? _dobrze & _aneksowana<>'N'
     || _dobrze:=0;
        {? ref_tab(_link)=UP
        || _um:=exec('FindAndGet','#table',UP,_link,,"UM",null());
           FUN.info('Umowy cykliczna %1 jest aneksowana.'@[exec('record','#to_string',_um)])
        || FUN.info('Umowy cykliczna %1 jest aneksowana.'@[exec('record','#to_string',_link)])
        ?}
     ?};

     {? _dobrze
::       Ustalenie parametrów sesji
     || __PARSES.setVal('OddzialLogProd',_oddz)
     || return(0)
     ?}

   ?};

   AreaTitle.setTabWin(UM,~~);
   AreaTitle.setTitle();
   {? _link <> ''
   || {? ref_tab(_link)=UM & UM.seek(_link)
      || UM.select(,1,5,,,,1)
      |? ref_tab(_link)=UP & UP.seek(_link) & UM.seek(UP.UM)
      || _f:=$('{? UM.ref() || exec(\'umowa_pozycje\',\'umowy\',\'%1\')?}'[_link]);
         UM.select(,1,5,,,_f,1)
      || return(0)
      ?}
   || UM.select()
   ?}

|| _far:="exec('before_umm','lum')";
   _win_grp:=UMM.grp_make(,_far,'umowymiejs_grp');

:: UMOWY MIEJSKIE
   _far:="exec('aftref_umm','lum')"; _fb:="exec('before_umm','lum')"; _fa:="exec('after_umm','lum')";
   UMM.grp_sel(_win_grp,UMM,'WER','Umowy miejskie'@,_far,,,22,_fb,_fa,,,'maximized');
   UMM.tab_splt(_win_grp,,'horizontal','info');
   _fb:="exec('set_efld_opt','umowy_miejskie','INFO')";
   UMM.grp_edit(_win_grp,UMM,'INFO',,,_fb,,,'maximized');
:: OKRESY GENERACJI ZGŁOSZEŃ (HGEN)
   _far:="exec('aftref_hgen','lum')"; _fb:="exec('before_hgen','lum')"; _fa:="exec('after_hgen','lum')";
   UMM.grp_sel(_win_grp,HGEN,'WER','Generowanie zgłoszeń'@,_far,,,22,_fb,_fa,,,'maximized');
   task_attach('LUM_UMO_HGEN');

   exec('init','lum');
   AreaTitle.setTabWin(UMM,~~);
   AreaTitle.setTitle();
   UMM.win_sel(_win_grp);
   UMM.select()
?};
