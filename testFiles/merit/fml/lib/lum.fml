      UM.tab_splt(_win_grp,,'horizontal','info');
      _fb:="exec('set_efld_opt','umowy','INFO',UMANGR)";
      UM.grp_edit(_win_grp,UM,'INFO',,,_fb,,,'maximized')
   ?};

:: OKRESY GENERACJI ZGŁOSZEŃ (HGEN)
   _far:="exec('aftref_hgen','lum')"; _fb:="exec('before_hgen','lum')"; _fa:="exec('after_hgen','lum')";
   UM.grp_sel(_win_grp,HGEN,'WER','Generowanie zgłoszeń'@,_far,,,22,_fb,_fa,,,'maximized');
