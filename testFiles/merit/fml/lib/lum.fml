\lum_umo_param_proc
::----------------------------------------------------------------------------------------------------------------------
::  UTW: [rr] [19.22]
:: OPIS: Zmiana parametrów pracy obszaru LUM_UMO
::----------------------------------------------------------------------------------------------------------------------
_oddz:=ST.ODDZ;
_okr_ref:=ST.OKR_REF;
{? AreaTitle.area='LUM_ZGL'
||
   _env:=params_get().env_zgl;
   {? __PARSES.editPar('OkresRok','OddzialLogProd')
   ||
      {? _env.TAB='jednorazowe'
      ||
         ZLP.index('R');
         ZLP.prefix('N');
         exec('zlp_actions','umowy_zlecenia','WER_ZGL')

      |? _env.TAB='wszystkie'
      ||
         ZLP.index('UZUP');
         ZLP.prefix();
         exec('zlp_actions','umowy_zlecenia','UZUP')

      |? _env.TAB='z_umow'
      ||
         ZLP.index('TFAK');
         ZLP.prefix('T','T','N');
         exec('zlp_actions','umowy_zlecenia','UZUP')
      ?};
      AreaTitle.setTitle()
   ?}

|? AreaTitle.area='LUM_FAK'
||
   _param:=$ST.AR+$ST.AM+ST.ODDZ;
   {? __PARSES.editPar('OkresRok','OddzialLogProd')
   ||
      {? _param<>$ST.AR+$ST.AM+ST.ODDZ
      ||
         _env:=params_get().env_fak;
         _czyFaks:=cur_tab(1,1)=FAKS;
         {? _czyFaks || _env.REF_U:=_env.REF_Z:=null() ?};
         {? _czyFaks
         ||
            FAKS.index('FPACZKA');
            _ref:={? _env.RODZ='U' || _env.REF_U || _env.REF_Z ?};
            {? _ref=null() || _ref:=-1 ?};
            FAKS.prefix(_ref)

         ||
            FPACZ.index('ROK');
            FPACZ.prefix(ST.ODDZ,ST.AR,_env.RODZ);
            _ref:={? _env.RODZ='U' || _env.REF_U || _env.REF_Z ?};
            FPACZ.seek(_ref)
         ?};
         AreaTitle.setTitle()
      ?}
   ?}
|? AreaTitle.area='LUO_KPO'
|| ZLP.f_clear();
   ZLP.prefix();
   __PARSES.editPar('OkresRok','OddzialLogProd');
   {? VAR.A_TABKPO='ZKPO'
   || exec('before_luokpo_zgl','luo')
   |? VAR.A_TABKPO='ZKPOK'
   || exec('before_luokpok_zgl','luo')
   ?};
   AreaTitle.setTitle()
|? __PARSES.editPar('OkresRok','OddzialLogProd')
||
   {? _oddz<>ST.ODDZ || exec('init','lum') ?};
   exec('before_allum','lum');
   AreaTitle.setTitle()
?}


