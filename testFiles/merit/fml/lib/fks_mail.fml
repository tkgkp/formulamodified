\ini_tmp
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AMK [8.60]
:: OPIS: Inicjowanie tabelki tymczasowej z trescia maila
::  OLD: \ini_tmp/mail_fk.fml
::----------------------------------------------------------------------------------------------------------------------
FMAILTMP:=tab_tmp(4,'POZ','INTEGER','Pozycja',
                    'OPIS1','STRING[80]','Opis 1',
                    'OPIS2','STRING[80]','Opis 2',
                    'OPIS3','STRING[80]','Opis 3',
                    'OPIS4','STRING[80]','Opis 4',
                    'OPIS5','STRING[80]','Opis 5',
                    'OPIS6','STRING[80]','Opis 6')


\add_tmp
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AMK [8.60]
:: OPIS: Dodawanie rekordow do tabelki tymczasowej z trescia maila
::   WE: _a - typ, _b,_c,_d,_e,_f,_g - opisy
::  OLD: \add_tmp/mail_fk.fml
::----------------------------------------------------------------------------------------------------------------------
{? var_pres('_b')<=0 || _b:='' ?};
{? var_pres('_c')<=0 || _c:='' ?};
{? var_pres('_d')<=0 || _d:='' ?};
{? var_pres('_e')<=0 || _e:='' ?};
{? var_pres('_f')<=0 || _f:='' ?};
{? var_pres('_g')<=0 || _g:='' ?};
FMAILTMP.POZ:=_a;
FMAILTMP.OPIS1:=_b;
FMAILTMP.OPIS2:=_c;
FMAILTMP.OPIS3:=_d;
FMAILTMP.OPIS4:=_e;
FMAILTMP.OPIS5:=_f;
FMAILTMP.OPIS6:=_g;
FMAILTMP.add()

