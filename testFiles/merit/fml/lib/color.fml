\init_kol_start
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AMK [17.00]
:: OPIS: Inicjowanie kolorów na starcie systemu
::----------------------------------------------------------------------------------------------------------------------
KOLTMP.cntx_psh(); KOLTMP.index('INDEX1'); KOLTMP.prefix(app_info('app_user'));
{? ~KOLTMP.first() & exec('done','zws') || exec('init','color') ?};
KOLTMP.cntx_pop();
KOLIKO.cntx_psh(); KOLIKO.index('INDEX1'); KOLIKO.prefix(app_info('app_user'));
{? ~KOLIKO.first() & exec('done','zws')
|| {? KOLIKO.lock(2)
   || exec('ini_leg','color');
      KOLIKO.unlock()
   ?}
?};
KOLIKO.cntx_pop()


\odtw_kolor_iko
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AMK [17.00]
:: OPIS: Odtwarzanie kolorów i ikon - czynność administracyjna
::   WE: [_a] - 0-z zapytaniem (domyślnie), 1-bez i dla bieżącego, 2-bez i dla wszystkich
::----------------------------------------------------------------------------------------------------------------------
{? var_pres('_a')<>type_of(0) | (_a>2 | _a<0) || _a:=0 ?};

_ok:={? ~_a
     || FUN.choice('Odtworzyć ustawienia kolorów i ikon?'@,,'Dla &bieżącego operatora'@,'Dla &wszystkich'@)
     || _a
     ?};
{? _ok
|| KOLTMP.cntx_psh(); KOLTMP.index('INDEX1');
   {? _ok=1
   || KOLTMP.prefix(app_info('app_user'));
      {? KOLTMP.first() || {! |? KOLTMP.del() !} ?}
   || KOLTMP.prefix();
      KOLTMP.erase()
   ?};
   {? _a=2
   || USERS.cntx_psh();
      USERS.index('USR_AKOD');
      USERS.prefix('T');
      {? USERS.first()
      || exec('PROGRESS','#object');
         PROGRESS.set(USERS.size(),'Aktualizacja ustawień kolorów ...'@,FUN.TYT);
         {!
         |? USERS.cntx_psh();
            Color.zmien_user(USERS.KOD);
            exec('init','color');
            USERS.cntx_pop();
            PROGRESS.next();
            USERS.next()
         !};
         PROGRESS.close()
      ?};
      USERS.cntx_pop();
      Color.zmien_user(app_info('app_user'))
   || exec('init','color')
   ?};
   KOLTMP.cntx_pop();
   KOLIKO.cntx_psh(); KOLIKO.index('INDEX1');
   {? _ok=1
   || KOLIKO.prefix(app_info('app_user'));
      {? KOLIKO.lock(2)
      || {? KOLIKO.first() || {! |? KOLIKO.del() !} ?};
         KOLIKO.unlock()
      ?}
   || KOLIKO.prefix();
      KOLIKO.erase()
   ?};
   {? _a=2
   || USERS.cntx_psh();
      USERS.index('USR_AKOD');
      USERS.prefix('T');
      {? USERS.first()
      || exec('PROGRESS','#object');
         PROGRESS.set(USERS.size(),'Aktualizacja ustawień ikon ...'@,FUN.TYT);
         {!
         |? USERS.cntx_psh();
            Legend.zmien_user(USERS.KOD);
            exec('ini_leg','color');
            USERS.cntx_pop();
            PROGRESS.next();
            USERS.next()
         !};
         PROGRESS.close()
      ?};
      USERS.cntx_pop();
      Legend.zmien_user(app_info('app_user'))
   || exec('ini_leg','color')
   ?};
   KOLIKO.cntx_pop()
?};
{? ~_a & _ok<>0 || FUN.info('Wykonano odtwarzanie kolorów i ikon.'@) ?};
''


