\os_zwzal_ar
::----------------------------------------------------------------------------------------------------------------------
::  UTW: DG [21.37]
:: OPIS: Formuła sprawdzająca poprawność wiersza w tabeli ze zwolnieniami od podatku.
::   WE: [_a] [STRING] - Znacznik operacji (D-dołącz/P-popraw, domyślnie 'P')
::   WY:
::  OLD: \os_zwzal_ar/kali.fml
::  OLD: \os_zwzal_ar/!pkd_dos_prus.fml
::----------------------------------------------------------------------------------------------------------------------
_action:={? var_pres('_a')=type_of('') || _a || 'P' ?};

OS_ZWSLO.cntx_psh();
_roczne:=(OS_ZWZAL.OS_ZWSLO().ROCZNE='T');
OS_ZWSLO.cntx_pop();
_result:=__CHK.record2(OS_ZWZAL,'OS_ZWSLO',,{? _roczne || 'ROK' || '' ?},,'D_ZO',,'D_OB');

{? _result='' & _roczne
|| _d_ob:=date(OS_ZWZAL.ROK,1,1);
   {? OS_ZWZAL.ROK<2022
   || FUN.emsg('Wartość w polu "Rok" nie może być mniejsza niż 2022.'@);
      _result:='ROK'
   |? OS_ZWZAL.D_ZO~1>OS_ZWZAL.ROK
   || FUN.emsg('Data w polu "Data złożenia" nie może być późniejsza od daty %1.'@[date(OS_ZWZAL.ROK,12,0)$1]);
      _result:='D_ZO'
   |? OS_ZWZAL.D_OB<_d_ob
   || FUN.emsg('Data w polu "Data stosowania" nie może być wcześniejsza od daty %1.'@[_d_ob$1]);
      _result:='D_OB'
   |? OS_ZWZAL.D_OB~1>OS_ZWZAL.ROK
   || FUN.emsg('Data w polu "Data stosowania" nie może być późniejsza od daty %1.'@[date(OS_ZWZAL.ROK,12,0)$1]);
      _result:='D_OB'
   ?}
?};
{? _result=''
|| {? OS_ZWZAL.D_ZO>OS_ZWZAL.D_OB
   || FUN.emsg('Data w polu "Data stosowania" nie może być wcześniejsza od daty w polu "Data złożenia".'@);
      _result:='D_OB'
   |? exec('os_zwzal_exist','!pkd_ezk_orwp',_action)
   || _txt:='W systemie istnieje już zapis dla podanej wartości pola "Data stosowania" i rodzaju wniosku.'@;
      {? ~FUN.ask('%1\n%2'[_txt,'Czy kontynuować?'@])
      || _result:='D_OB'
      ?}
   ?}
?};
:: Kontrola wycofania w stosunku do wycofywanego:
{? _result='' & OS_ZWZAL.WYCOF='T'
|| _dZoOjca:=_dObOjca:=date(0,0,0);
   OS_ZWZAL.cntx_psh();
   {? OS_ZWZAL.seek(OS_ZWZAL.DOK_WYC,,1)
   || _dZoOjca:=OS_ZWZAL.D_ZO;
      _dObOjca:=OS_ZWZAL.D_OB
   ?};
   OS_ZWZAL.cntx_pop();
   {? OS_ZWZAL.D_ZO<=_dZoOjca
   || FUN.emsg('Data złożenia wycofania nie może być mniejsza od daty złożenia wycofywanego dokumentu.'@);
      _result:='D_ZO'
   |? OS_ZWZAL.D_OB<=_dObOjca
   || FUN.emsg('Data stosowania wycofania nie może być mniejsza od daty stosowania wycofywanego dokumentu.'@);
      _result:='D_OB'
   ?}
?};

_result


