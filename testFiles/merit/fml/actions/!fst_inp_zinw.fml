\srxl_przew
::----------------------------------------------------------------------------------------------------------------------
::  UTW: PJ [17.00]
:: OPIS: Obsługa wyboru przewodniczącego
::----------------------------------------------------------------------------------------------------------------------
{? exec('srxk_arch','!fst_inp_zinw',SRXK.ref())
|| FUN.emsg('Komisja związana z zamkniętym arkuszem, modyfikacje niemożliwe.'@);
   return()
?};
_ref:=SRXL.ref();
SRXL.cntx_psh();
SRXL.index('P');
SRXL.prefix(SRXL.SRXK,'T');
{? ~SRXL.first()
|| {? FUN.ask('Przypisać funkcję przewodniczącego komisji?'@)
   || SRXL.P:='T';
      SRXL.clear();
      SRXL.put()
   ?}
|? SRXL.ref=_ref
|| {? FUN.ask('Wskazana osoba już przewodniczy komisji.\n'
             +'Zmienić na członka komisji?'@)
   || SRXL.P:='N';
      SRXL.clear();
      SRXL.put()
   ?}
|| {? FUN.ask('Inna osoba już przewodniczy komisji.\n'
             +'Zmienić osobę przewodniczącą komisji?'@)
   || SRXL.P:='N';
      SRXL.clear();
      SRXL.put();
      {? SRXL.seek(_ref)
      || SRXL.P:='T';
         SRXL.put()
      ?}
   ?}
?};
SRXL.cntx_pop();
1


