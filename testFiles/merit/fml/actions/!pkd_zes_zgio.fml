::# permissions=F_ZATR,UD_SKL

{? FUN.ask('Funkcja generuje dokumenty zawierające informacje o udostępnieniu danych osobowych'@+'\n'+
      'Czy kontynuować?'@)
|| _prac:=exec('wybierz_parses','pracownik','PKD');
   {? _prac.P.first()
   || {!
      |? {? P.seek(_prac.P.SQL,,1)
         || P.OSOBA();
            _wydruk:=username+$P.tm_stamp+'.pdf';
            {? 1+_wydruk='@' || _wydruk:=1-_wydruk ?};
            {? rep_exec('pkd_xxx_giodo',,0,_wydruk,1) || exec('add_zal','zal','gi',,_wydruk) ?}
         ?};
         _prac.P.next()
      !}

   ?}
?};
