\ecb_xml_today
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AFI [21.14_09]
:: OPIS: Formuła przetwarza plik xml z tabelą średnich kursów walut dla ECB na dzisiaj
::----------------------------------------------------------------------------------------------------------------------
_adr:='https://www.ecb.europa.eu/stats/eurofxref/eurofxref-daily.xml';
_ht:=inet_get(_adr);
_ret:=_ht.http_get();
{? _ret<>200
|| return('Kod odpowiedzi: %1' [$_ret])
?};
_res:=_ht.get_data();

inTkrsTab:=0;
inKrsTab:=0;
contParse:=1;

xml_sax_parse(_res,,,
   "{? contParse
    || {? _a='Cube' & var_press('_e') & _e[1]='time'
       || TKRS.index('TKRS_SM'); TKRS.prefix();
          _tabSym:=(_e[4]+2)+'/'+((7+_e[4])+2)+'/ECB/'+(4+_e[4]);
          {? ~TKRS.find_key(POMOC.BANK,POMOC.WAL_TKRS,_tabSym)
          || TKRS.blank();
             inTkrsTab:=1;
             TKRS.DT:=date(#(4+_e[4]),#((7+_e[4])+2),#(_e[4]+2));
             TKRS.SYM:=(_e[4]+2)+'/'+((7+_e[4])+2)+'/ECB/'+(4+_e[4])
          || FUN.info('Tabela kursów o symbolu '+_tabSym+' jest już zaimportowana.'@);
             contParse:=0;
             0
          ?}
       ?}
    ?};
    {? contParse
    || {? _a='Cube' & var_press('_e')>0 & _e[1]='currency' & _e[5]='rate'
       || KRS.prefix(refTkrs);
          KRS.blank();
          KRS.TKRS:=refTkrs;
          inKrsTab:=1;
          {? SLO.find_key(_e[4])
