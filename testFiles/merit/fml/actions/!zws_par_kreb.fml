{? var_pres('B_VIUDO',FINFO)>0
|| exec('czytaj','#stalesys',,FINFO);

   BANORG.cntx_psh(); B.cntx_psh();
   BANORG.index('NDX');
   BANORG.prefix('ECB','Europejski Bank Centralny',);
   _org:=_bank:=null;

   {? BANORG.first()
   || _org:=BANORG.ref()
   ?};
   {? _org
   || B.index('BANK');
      B.prefix('Europejski Bank Centralny - Centrala','00000001');
      {? B.first()
      || _bank:=B.ref()
      ?}
   ?};
   {? FINFO.SLWAL().SLU=null()
   || FUN.info('Nie wskazano słownika walut w słownikach systemu.'@)
   |? ~(_org & _bank)
   || FUN.info('Kartoteka banków systemu nie zawiera Europejskiego Banku Centralnego.'@)
   |? FINFO.B_VIUDO=null()
   || FUN.info('Brak wypełnionego pola Bank dla VIU-DO o Europejski Bank Centralny w parametryzacji systemu.'@)
   || POMOC.BANK:=FINFO.B_VIUDO;
      SLU.cntx_psh(); SLO.cntx_psh();
      SLO.index('SL');
      _wal:=null;
      SLO.prefix(FINFO.SLWAL().SLU,'EUR');
      {? SLO.first()
      || _wal:=SLO.ref()
      ?};
      SLU.cntx_pop(); SLO.cntx_pop();
      {? _wal=null()
      || FUN.info('Słownik walut w słownikach systemu nie zawiera waluty Euro.'@)
      || exec('eur_tkrs','waluty',_wal,_bank);
         1
      ?}
   ?};
   BANORG.cntx_pop(); B.cntx_pop()
?};
