\action_modify_gpr
::----------------------------------------------------------------------------------------------------------------------
::  UTW: AKUL [23.25]
:: OPIS: Grupa przed dla akcji Popraw w oknach wertowania dla wzorców technologicznych
::----------------------------------------------------------------------------------------------------------------------
_continue:=1;
TKTL.cntx_psh();
{? TKTL.sel_size()>1
|| _continue:=FUN.ask('Ilość zaznaczonych wzorców technologii: %1. Czy poprawić wybrane wzorce kart technologicznych?'@
      [form(TKTL.sel_size())]);
   {? _continue>0
   ||
      TKTL.seek(null(),,,1);
      TKTL.blank(1);
      VAR.GRP_MOD:='P';
      {? _continue>0
      || _buffer:=exec('obj_new','#buf','TKTL');
         _buffer.get();
         params_set('buffer',_buffer);
         TKTL.win_edit('GRP_POP_');
         _valid:="
            _res:=1;
            params_set(params_get());
            _buffer:=exec('obj_new','#buf','TKTL');
            _buffer.get();
            {? exec('compare','#table',_buffer,params_get().buffer,0,'GKTL')
            || FUN.info('Przynajmniej jedno z pól musi zostać określone.'@);
               _res:=0
            ?};
            _res
         ";
         _continue:=TKTL.edit(_valid);
         {? _continue>0
         || _buffer_grp:=exec('obj_new','#buf','TKTL');
            _buffer_grp.get();
            {? var_pres('__BUFF_GRP')>100
            || VAR_DEL.delete('__BUFF_GRP')
            ?};
            __BUFF_GRP:=_buffer_grp;
            sel_nchk();
            KOMM.init(255,,'Grupowa modyfikacja wzorców kart technologicznych'@);
            VAR.GRP_MOD:='T';
            VAR.GRUPA:='T'
         || VAR.GRP_MOD:='N';
            VAR.GRUPA:='N';
            {? var_pres('__BUFF_GRP')>100
            || VAR_DEL.delete('__BUFF_GRP')
            ?}
         ?}
      ?}
   ?}
?};
TKTL.cntx_pop();
_continue


