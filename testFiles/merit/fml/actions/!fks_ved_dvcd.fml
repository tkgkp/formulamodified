::   WY:
::  OLD: \tab2poz/cit.fml
::----------------------------------------------------------------------------------------------------------------------
_nr:=VAT_DEK.NR().NR;
{? _nr>29
|| _save:=obj_new(obj_len(_b));
   {! _ii:=1..obj_len(_save)
   |! _save[_ii]:=1
   !}
?};
{? var_pres('_b')>0
|| VAT_POZ.cntx_psh();
   VAT_POZ.index('VAT_POZ'); VAT_POZ.prefix(VAT_DEK.ref(),_a);
   {? VAT_POZ.first()
   || {!
      |? _put:=0;
         _ii:={? VAT_POZ.POZ1 || VAT_POZ.POZ1 || VAT_POZ.POZ2 ?};
         {? var_pres('['+$_ii+']', _b)>0 & var_pres('['+$_ii+']', _b)<>type_of(1) & _nr>=32
         || _litera:=#(VAT_POZ.LP+1)=0 & VAT_POZ.LP+1<>'0';
            {? VAT_POZ.POZ1
            || VAT_POZ.NETTO:={? _litera || _b[_ii].NA || _b[_ii].N ?};
               _put:=1
            ?};
            {? VAT_POZ.POZ2
            || VAT_POZ.VAT:={? _litera || _b[_ii].VA || _b[_ii].V ?};
               _put:=1
            ?}
         || {? VAT_POZ.POZ1 & _b[VAT_POZ.POZ1]<>~~ & (var_pres('_save')<=0 | _save[VAT_POZ.POZ1])
            || VAT_POZ.NETTO:=_b[VAT_POZ.POZ1];
               _put:=1;
               {? var_pres('_save')>0
               || _save[VAT_POZ.POZ1]:=0
               ?}
            ?};
            {? VAT_POZ.POZ2 & _b[VAT_POZ.POZ2]<>~~ & (var_pres('_save')<=0 | _save[VAT_POZ.POZ2])
            || VAT_POZ.VAT:=_b[VAT_POZ.POZ2];
               _put:=1;
               {? var_pres('_save')>0
               || _save[VAT_POZ.POZ2]:=0
               ?}
            ?}
         ?};
         {? _put || VAT_POZ.put() ?};
         VAT_POZ.next()
      !}
   ?};
   VAT_POZ.cntx_pop()
?}


