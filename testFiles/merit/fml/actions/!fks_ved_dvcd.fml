\akt_spr
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [12.41]
:: OPIS: Aktualizacja sprawozdania przed utworzeniem deklaracji CIT
::   WE: _a - skrot sprawozdania
::  OLD: \akt_spr/cit.fml
::----------------------------------------------------------------------------------------------------------------------
GR_SIK.cntx_psh();
GR_SIK.index('SYSSKROT');
GR_SIK.prefix(REF.FIRMA,'FKS',_a,);
{? GR_SIK.first()
|| SIK.GR_SIK:=GR_SIK.ref();
   OKRESY.ROK1:=SIK.ROK2;

   OKRESY.OKRES_OD:=SIK.OKRROK2;
   OKRESY.OKRES_DO:=SIK.OKRROK2;
   OKRESY.AKT_POW:='T';
   OKRESY.CZYMC:='T';
   AUTO:=0;
   __OneF:=0;
   GR_SIK.prefix();
   exec('aktualizuj','sprfin');
   VAR_DEL.delete('AUTO','__OneF')
?};
GR_SIK.cntx_pop()


\gen_poz
::----------------------------------------------------------------------------------------------------------------------
::  UTW: [MB] [2008]
:: OPIS: Tworzenie pozycji deklaracji
::   WE:  _a  - skrot sprawozdania
::       [_b] - prefiks kodu
::  OLD: \gen_poz/cit.fml
::----------------------------------------------------------------------------------------------------------------------
{? var_pres('_b')<0 || _b:='' ?};
_cit8_2k:=4+VAT_DEK.TYP='CIT8' & VAT_DEK.NR().NR>=26;
GR_SIK.index('SYSSKROT'); GR_SIK.prefix(REF.FIRMA,'FKS',_a,);
GR_SIK.first();
SIK.OKRROK2().ROK();
GR_KOL.index('LP'); GR_KOL.prefix(GR_SIK.ref());
{? GR_KOL.first()
|| {? _cit8_2k
   || _k1:=GR_KOL.ref();
      {? GR_KOL.next()
      || _k2:=GR_KOL.ref()
      || _k2:=null
      ?}
   ?};
   DEFW.index('LP'); DEFW.prefix(GR_SIK.ref());
   OSPR.index('OKRES2'); OSPR.prefix(REF.FIRMA,'M',ROK_F.ref(),OKRO_F.NR);
   {? OSPR.first()
   || WARTOSCI.cntx_psh(); WARTOSCI.use('wsik_'+ROK_F.KOD);
      WARTOSCI.index('OKRES'); WARTOSCI.prefix(OSPR.ref(),REF.WYKON);
      VAT_POZ.index('VAT_POZ'); VAT_POZ.prefix(VAT_DEK.ref());
      {? DEFW.first()
      || _cit8:=6+_b<>'CIT8/O';
         {!
         |? {? DEFW.ALGORYTM<>'P'
            || VAT_POZ.VAT_DEK:=VAT_DEK.ref();
               _kod:={? DEFW.KOD+1='.' || DEFW.KOD-1 || DEFW.KOD ?};
               _lit:='';
               {? 'abc'*(_kod+1)
               || _lit:=_kod+1;
                  _kod:=_kod-1
               ?};
               VAT_POZ.LP:=_b+(('00'+_kod)+3)+_lit;
               VAT_POZ.TYP:=exec('poz_typ','!fks_ved_dvcd',#_kod,_b,_lit);
               VAT_POZ.OP:=DEFW.NAZWA;
               {? _cit8_2k
               || _poz:={? 'abc'*((DEFW.KOD-1)+1) || #(DEFW.KOD-2)|| #(DEFW.KOD-1) ?};
                  _dwie:=exec('cit8_2k','fks_ved',_cit8);
                  {? VAT_POZ.TYP='T' | VAT_POZ.TYP='D'
                  || VAT_POZ.POZ1:=0;
                     VAT_POZ.NETTO:=0;
                     VAT_POZ.POZ2:=_poz;
                     VAT_POZ.VAT:=0
                  || {? _dwie
                     || VAT_POZ.POZ1:=_poz;
                        VAT_POZ.NETTO:={? WARTOSCI.find_key(DEFW.ref(),_k1,OPERATOR.DEPT) || WARTOSCI.WARTOSC || 0 ?};
                        VAT_POZ.POZ2:=_poz+1;
                        VAT_POZ.VAT:={? WARTOSCI.find_key(DEFW.ref(),_k2,OPERATOR.DEPT) || WARTOSCI.WARTOSC || 0 ?}
                     || VAT_POZ.POZ1:=0;
                        VAT_POZ.NETTO:=0;
                        VAT_POZ.POZ2:=_poz;
                        VAT_POZ.VAT:={? WARTOSCI.find_key(DEFW.ref(),_k2,OPERATOR.DEPT) || WARTOSCI.WARTOSC || 0 ?}
                     ?}
                  ?}
               || VAT_POZ.NETTO:={? WARTOSCI.find_key(DEFW.ref(),GR_KOL.ref(),OPERATOR.DEPT) || WARTOSCI.WARTOSC || 0 ?};
                  VAT_POZ.POZ1:={? DEFW.KOD*'-' || #((DEFW.KOD*'-')+DEFW.KOD-1) |? DEFW.KOD+1='.' || #(DEFW.KOD-1) || #DEFW.KOD ?};
                  VAT_POZ.VAT:=0
               ?};
               VAT_POZ.TEXT:='';
               VAT_POZ.add()
            ?};
            DEFW.next()
         !}
      ?};
      WARTOSCI.cntx_pop()
   ?}
?}


\poz_typ
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [12.41]
:: OPIS: Typ pozycji VAT
::   WE: _a - pozycja
::       _b - prefiks
::       _c - litera
::  OLD: \poz_typ/cit.fml
::----------------------------------------------------------------------------------------------------------------------
_nr:=VAT_DEK.NR().NR;
_lit:={? var_press('_c')>0 || _c || '' ?};
{? _b=''
|| {? DEFW.ALGORYTM='R' || 'R' || 'A' ?}
|? _b='CIT8   '
|| {? _nr>=32
   || {? ',60,76,78,80,84,86,90,92,94,96,106,108,110,112,114,115,116,118,119,120,131,132,137,138,139,141,143,144,145,146,147,227,233,239,245,246,247,248,249,259,'*(','+$_a+',')
      || 'S'
      |? ',135,'*(','+$_a+',')
      || 'T'
      |? _a>315 & _a<319
      || 'C'
      |? _a>=320 & _a<322
      || 'C'
      |? DEFW.ALGORYTM='R'
      || 'R'
      || 'A'
      ?}
   |? _nr>=31
   || {? ',58,76,78,80,84,86,90,92,94,96,106,108,110,112,114,115,116,118,119,130,131,136,137,138,140,142,143,144,145,146,226,232,238,244,245,246,247,248,255,256,'*(','+$_a+',')
      || 'S'
      |? ',134,'*(','+$_a+',')
      || 'T'
      |? _a>312 & _a<316
      || 'C'
      |? _a>317 & _a<319
