\set_vat_dek
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [17.00]
:: OPIS: Przed dodaniem deklaracji - ustawienie danych
::----------------------------------------------------------------------------------------------------------------------
_typ:=SKID.TYP_ZAP:=SKID.DEKL_NAZ:='CIT8';
VAT_DEK.blank(1);
VAT_DEK.ROK:=SIK.ROK2;
VAT_DEK.RODZAJ:=1+SKID.TYP_ZAP;
_rok:=SIK.OKRROK2().POCZ~1;
_mc:=SIK.OKRROK2().POCZ~2;
VAT_DEK.OKRES:=$_rok+'/'+{? _mc<10 || '0' || '' ?}+$_mc;
_wer:=0;
VAT_DEK.cntx_psh();
VAT_DEK.index('VAT_DEK'); VAT_DEK.prefix(ROK_F.POCZ_ROK,SKID.TYP_ZAP,VAT_DEK.OKRES);
{? VAT_DEK.last()
|| _ok:=FUN.choice(
      'Istnieje już deklaracja w okresie %1 %2.'@[OKRO_F.NAZ,ROK_F.NAZ],1,
      'utwórz Nową'@,'utwórz Korektę'@
   );
   _wer:=VAT_DEK.WER
|| _ok:=1
?};
{? _ok=2 || _typ:=_typ+'_KOR' ?};
{? _ok>1
|| VAT_DEK.index('VAT_DEK'); VAT_DEK.prefix(ROK_F.POCZ_ROK,_typ,VAT_DEK.OKRES);
   _wer:={? VAT_DEK.last() || VAT_DEK.WER || 0 ?}
?};
VAT_DEK.cntx_pop();
{? _ok
|| _skrot:=exec('cit_gr_sik','!fks_ved_dvcd');
   _skrot1:=_skrot.skrot1;
   _skrot2:=_skrot.skrot2;
   {? _skrot1<>''
   || _fun:="
         FUN.emsg('Sprawozdanie '''+_a+''' na którym bazuje\ndeklaracja nie '+{? _b=0 || 'istnieje' || 'jest zaakceptowane' ?}+'.')
      ";
      GR_SIK.cntx_psh(); GR_SIK.index('SYSSKROT'); GR_SIK.prefix(REF.FIRMA,'FKS');
      {? GR_SIK.find_key(_skrot1)
      || {? GR_SIK.AKC<>'T'
         || _fun(_skrot1,1);
            _ok:=0
         ?}
      || _fun(_skrot1,0);
         _ok:=0
      ?};
      {? _ok=1 & _skrot2<>''
      || {? GR_SIK.find_key(_skrot2)
         || {? GR_SIK.AKC<>'T'
            || _fun(_skrot2,1);
               _ok:=0
            ?}
         || _fun(_skrot2,0);
            _ok:=0
         ?}
      ?};
      GR_SIK.cntx_pop();
      {? _ok
      || OSPR.cntx_psh();
         OSPR.index('OKRES2'); OSPR.prefix(REF.FIRMA,'M',ROK_F.ref(),OKRO_F.NR); _ospr:=OSPR.first();
         OSPR.cntx_pop();
         {? _ospr=0
         || FUN.info('Brak miesięcznych okresów raportowania dla firmy %1'
                     ' w %2 roku.'@[REF.FIRMA().SYMBOL,SIK.OKRROK2().ROK().NAZ]
            )
         ?};
         _ok:=_ospr
      ?}
   ?};
   {? _ok
   || VAT_DEK.TYP:=_typ;
      VAT_DEK.WER:=_wer+1;
      VAT_DEK.DATA:=date();
      VAT_DEK.KOR:={? _ok=1 || 'N' || 'T' ?};
      _citnr:=SKID.TYP_ZAP+1;
      VAT_DEK.OPIS:={? _ok=1 || 'Deklaracja CIT-'+_citnr |? _ok=2 || 'Korekta CIT-'+_citnr ?};
      _d:=date((#(4+$OKRO_F.POCZ)),#((7+$OKRO_F.POCZ)+2),0);
      VAT_VER.index('VER_OD'); VAT_VER.prefix(BPMN.SYM_DOM,SKID.DEKL_NAZ,SKID.DEKL_NAZ);
      VAT_DEK.NR:={? VAT_VER.find_le(_d) || VAT_VER.ref() || null ?};
      _nr:=VAT_DEK.NR().NR;
      VAT_DEK.win_edit('CIT8_2');
      _nr:=VAT_DEK.NR().NR;
      {? _nr>=32
      || {? var_pres('PAR12',VAT_DEK)<=0
         || _ok:=0; FUN.info('Brak naniesionej aktualizacji dla deklaracji CIT-8(32).')
         || exec('set_fld','fks_ved',1);
            VAT_DEK.PAR5:=VAT_DEK.PAR8:='N'; VAT_DEK.PAR12:='N';
            VAT_DEK.win_edit('CIT8_8')
         ?}
      |? _nr>=31
      || exec('set_fld','fks_ved',1);
         VAT_DEK.PAR5:=VAT_DEK.PAR8:='N'; VAT_DEK.PAR11:=2;
         VAT_DEK.win_edit('CIT8_7')
      |? _nr>=29
      || exec('set_fld','fks_ved',1);
         VAT_DEK.PAR5:=VAT_DEK.PAR8:='N';
         VAT_DEK.win_edit('CIT8_6')
      |? _nr>=28
      || exec('set_fld','fks_ved',1);
         VAT_DEK.PAR5:=VAT_DEK.PAR8:='N';
         VAT_DEK.win_edit('CIT8_5')
      |? _nr>=26
      || VAT_DEK.win_edit('CIT8_4')
      |? _nr>=25
      || VAT_DEK.win_edit('CIT8_3')
      |? _nr>=22
      || VAT_DEK.win_edit('CIT8_2')
      || VAT_DEK.win_edit('CIT8')
      ?}
   ?}
?};
_ok


\add
::----------------------------------------------------------------------------------------------------------------------
::  UTW: MB [17.00]
:: OPIS: Dodaje deklarację i jej pozycje
::----------------------------------------------------------------------------------------------------------------------
_skrot:=exec('cit_gr_sik','!fks_ved_dvcd');
_skrot1:=_skrot.skrot1;
_skrot2:=_skrot.skrot2;
GR_SIK.cntx_psh();
{? UNPAR.P1 & _skrot1<>''
|| exec('akt_spr','!fks_ved_dvcd',_skrot1)
?};
{? UNPAR.P2
|| VAT_DEK.cntx_psh();
   _from:={? VAT_DEK.get() || VAT_DEK.ref() || null ?};
   VAT_DEK.cntx_pop()
?};
VAT_DEK.prefix(); _ref:=null;
{? VAT_DEK.add()
|| _ref:=VAT_DEK.ref();
   _nr:=VAT_DEK.NR().NR;
   {? _nr>=33
   || exec('gen_poz','cit_zal','CIT8   ',0);
      {? VAT_DEK.ZAL4
      || exec('gen_poz','cit_zal','CIT8/O ',1)
      ?}
   || exec('gen_poz','!fks_ved_dvcd',_skrot1,'CIT8   ');
      {? VAT_DEK.ZAL4
      || exec('gen_poz','!fks_ved_dvcd',_skrot2,'CIT8/O ')
      ?}
   ?};
   {? UNPAR.P2 & _from
   || exec('copy','!fks_ved_dvcd',_from,_ref)
   ?};
   exec('oblicz_cit8','!fks_ved_dvcd');
   exec('vatuzas1','fks_ved','ZAL3','Uzasadnienie korekty deklaracji');
   {? VAT_DEK.NR().NR>=28
   || _warunek:="1";
      exec('vatuzas1','fks_ved','ZAW','Okresy zawiesz. wykon. działalności przez podatnika',_warunek);
      exec('vatuzas1','fks_ved','ZAW_J','Nazwy spółek i okresy zawiesz. wykon. dział. spółek jawnych',_warunek)
   ?};
   {? VAT_DEK.NR().NR>=30
   || _warunek:="1";
      exec('vatuzas1','fks_ved','INNE','Inne informacje',_warunek)
   ?}
?};
GR_SIK.cntx_pop();
{? _ref || VAT_DEK.seek(_ref) ?};
_ref


