   || _v1:=exec('vat_zd_size','fks_ved');
      _v2:=VAT_DEK.ZAL3;
      {? _v1+_v2>0
      || wer:=_wer;
         _popup:=
            'popup(0,\'Wydruk deklaracji\','+
