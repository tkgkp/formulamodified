::  UTW: IS [22.26]
:: OPIS: Uzupełnienie słownika podstaw prawnych nowymi zapisami.
::   WE:
::   WY:
::----------------------------------------------------------------------------------------------------------------------
_ret:=1;
_TAB:=tab_tmp(1
    ,'LP'      ,'STRING[2]'  ,'L.p. (obecna)'
    ,'LP_N'    ,'STRING[2]'  ,'L.p. (nowa)'
    ,'ARTYKUL' ,'STRING[10]' ,'Artykuł'
    ,'ART_NDX' ,'STRING[10]' ,'Numer artykułu - po apostrofie'
    ,'PARAGRAF','STRING[10]' ,'Paragraf'
    ,'PUNKT'   ,'STRING[10]' ,'Punkt'
    ,'TRESC'   ,'STRING[100]','Treść obecna'
    ,'TRESC_N' ,'STRING[100]','Treść nowa'
    ,'OPIS'    ,'SYS_MEMO'   ,'Treść i opis podstawy prawnej (obecna)'
    ,'OPIS_N'  ,'SYS_MEMO'   ,'Treść i opis podstawy prawnej (nowa)'
    ,'ZG_N'    ,'STRING[1]'  ,'Zgodna z nową definicją [T/N]'
    ,'ZG_S'    ,'STRING[1]'  ,'Zgodna ze starą definicją [T/N]'
    ,'UWAGI'   ,'STRING[255]','Uwagi'
    ,'PRZENIES','STRING[1]'  ,'Czy nanieść nową definicję do kartoteki? [T/N]'
    );
:: Znaki do wycięcia z opisów podczas porównywania:
_wytZn:=spli_str('\\\n,\\\t,\\\r, ');
_TAB.index(_TAB.ndx_tmp(,,'ARTYKUL',,0,'ART_NDX',,0,'PARAGRAF',,0,'PUNKT',,0));
_TAB.prefix();
_NOWE:=exec('wyp_slo_pdst_praw','upgrade_2226_nru01',0);
_NOWE.index(_NOWE.ndx_tmp(,,'ARTYKUL',,0,'ART_NDX',,0,'PARAGRAF',,0,'PUNKT',,0));
_NOWE.prefix();
_STARE:=exec('wyp_slo_pdst_praw','upgrade_2226_nru01',1);
_STARE.index(_STARE.ndx_tmp(,,'ARTYKUL',,0,'ART_NDX',,0,'PARAGRAF',,0,'PUNKT',,0));
_STARE.prefix();
RD_PPRAW.cntx_psh();
RD_PPRAW.index('P_PRAW_U');
RD_PPRAW.prefix();
:: Przejście po obecnych:
{? RD_PPRAW.first()
|| {!
   |? _TAB.blank();
      _TAB.LP:=RD_PPRAW.LP;
      _TAB.ARTYKUL:=RD_PPRAW.ARTYKUL;
      _TAB.ART_NDX:=RD_PPRAW.ART_NDX;
      _TAB.PARAGRAF:=RD_PPRAW.PARAGRAF;
      _TAB.PUNKT:=RD_PPRAW.PUNKT;
      _TAB.TRESC:=RD_PPRAW.TRESC;
      _opis:=RD_PPRAW.memo_txt(,1,'OPIS');
      _TAB.memo_set(_opis,'OPIS');
      _TAB.memo_set('','OPIS_N');
      _TAB.UWAGI:='';
      _TAB.PRZENIES:='N';
      {? _STARE.find_key(_TAB.ARTYKUL,_TAB.ART_NDX,_TAB.PARAGRAF,_TAB.PUNKT,)
      || {? gsub(_STARE.memo_txt(,1,'OPIS'),_wytZn,'')=gsub(_opis,_wytZn,'')
         || _TAB.ZG_S:='T'
         || _TAB.ZG_S:='N'
         ?}
      || _TAB.ZG_S:='N';
         _TAB.UWAGI+='Brak w definicji (starej) standardowej sprzed aktualizacji. '@
      ?};
      {? _NOWE.find_key(_TAB.ARTYKUL,_TAB.ART_NDX,_TAB.PARAGRAF,_TAB.PUNKT,)
      || _TAB.LP_N:=_NOWE.LP;
         _TAB.TRESC_N:=_NOWE.TRESC;
         _nOpis:=_NOWE.memo_txt(,1,'OPIS');
         _TAB.memo_set(_nOpis,'OPIS_N');
         {? gsub(_nOpis,_wytZn,'')=gsub(_opis,_wytZn,'')
         || _TAB.ZG_N:='T'
         || _TAB.ZG_N:='N'
         ?}
      || _TAB.ZG_N:='N';
         _TAB.UWAGI+='Brak w definicji (nowej) standardowej po aktualizacji. '@
      ?};
      {? _TAB.add()
      || _TAB.memo_put(,'OPIS');
         _TAB.memo_put(,'OPIS_N')
      ?};

      RD_PPRAW.next()
   !}
?};
RD_PPRAW.cntx_pop();
:: Przejście po "nowych", których nie ma jeszcze w systemie:
{? _NOWE.first()
|| {!
   |? {? ~_TAB.find_key(_NOWE.ARTYKUL,_NOWE.ART_NDX,_NOWE.PARAGRAF,_NOWE.PUNKT,)
      || _TAB.blank();
         _TAB.LP_N:=_NOWE.LP;
         _TAB.ARTYKUL:=_NOWE.ARTYKUL;
         _TAB.ART_NDX:=_NOWE.ART_NDX;
         _TAB.PARAGRAF:=_NOWE.PARAGRAF;
         _TAB.PUNKT:=_NOWE.PUNKT;
         _TAB.TRESC_N:=_NOWE.TRESC;
         _TAB.ZG_N:='N';
         _TAB.ZG_S:='N';
         _TAB.memo_set('','OPIS');
         _TAB.memo_set(_NOWE.memo_txt(,1,'OPIS'),'OPIS_N');
         _TAB.UWAGI:='Nowy zapis.';
         _TAB.PRZENIES:='N';
         {? _TAB.add()
         || _TAB.memo_put(,'OPIS');
            _TAB.memo_put(,'OPIS_N')
         ?}
      ?};

      _NOWE.next()
   !}
?};
_wer:=_TAB.mk_sel('Aktualizacja słownika podstaw prawnych urlopów'@,,0,'#slo_pdst_praw1',,,,,'U','T');
