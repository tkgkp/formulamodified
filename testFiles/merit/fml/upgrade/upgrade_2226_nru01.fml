            RD_PPRAW.TRESC:=_TAB.TRESC_N;
            RD_PPRAW.memo_set(_TAB.memo_txt(,1,'OPIS_N'),'OPIS');
            {? RD_PPRAW.put()
            || RD_PPRAW.memo_put(,'OPIS')
            ?}
         ?};

         _TAB.next()
      !};
      RD_PPRAW.cntx_pop();
      FUN.info('Nanoszenie zmian zakończone.'@)
   || FUN.info('Nie zaznaczono wpisów do naniesienia.'@)
   ?}
|| FUN.info('Zrezygnowano z dalszych działań.'@)
