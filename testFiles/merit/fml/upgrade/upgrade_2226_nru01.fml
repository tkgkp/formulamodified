_TAB.win_fld(_wer,,'LP_N',,,-10,,,'L.p. (nowa)'@,,'Znacznik grupujący wg nieobecności'@);
_TAB.win_fld(_wer,,'ZG_S',,,-16,,,'Zgodność ze starą definicją'@,,
