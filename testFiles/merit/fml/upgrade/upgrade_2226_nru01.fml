_TAB.win_fld(_wer,,'UWAGI',,,-30,,,'Uwagi'@,,'Uwagi'@);
_TAB.win_act(_wer,0,'Formuła','Zaznacz'@,,'Zmień stan przenoszenia wpisu'@,
