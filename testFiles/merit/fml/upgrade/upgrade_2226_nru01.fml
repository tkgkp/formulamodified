\pola
::----------------------------------------------------------------------------------------------------------------------
::  UTW: IS [22.26]
:: OPIS: Uzupełnienie wartości w nowych polach związanych z nowymi rodzajami urlopów.
::   WE:
::   WY:
::----------------------------------------------------------------------------------------------------------------------
_ret:=1;
_MSK:=H_UM.names();
_MSK.clear();
{? _MSK.first()
|| H_UM.cntx_psh();
   H_UM.trig_off('*','*');
   {!
   |? H_UM.use(_MSK.NAME);
      H_UM.prefix();
      {? H_UM.first()
      || {!
         |? _put:=0;
            {? ~(',T,N,'*',%1,'[H_UM.PRZ_USPR])
            || H_UM.PRZ_USPR:='N';
               _put:=1
            ?};
            {? ~(',T,N,'*',%1,'[H_UM.ZAM_ZAW])
            || H_UM.ZAM_ZAW:='N';
               _put:=1
            ?};
            {? _put
            || _ret*=H_UM.put()
            ?};

            H_UM.next()
         !}
      ?};

      _MSK.next()
   !};
   H_UM.trig_on('*','*');
   H_UM.cntx_pop()
?};
obj_del(_MSK);
{? _ret<>1
|| _ret:=-1
?};

{? _ret<>1
|| __UPG.msg('Nie powiodła się operacja uzupełnienia nowych pól.')
|| __UPG.msg('Operacja uzupełnienia nowych pól zakończona powodzeniem.')
?};

_ret


