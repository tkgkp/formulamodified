::----------------------------------------------------------------------------------------------------------------------
::  UTW: IS [22.26]
:: OPIS: Zasilenie słowników w dane startowe.
::   WE:
::   WY:
::----------------------------------------------------------------------------------------------------------------------
_ret:=1;
:: Oświadczenia wniosków o urlop opiekuńczy
{? (_typ:=exec('slo_typ','ext_slo','NWOPO'))<>null()
|| _update:=exec('slo_kod','dane_startowe',1);
   {? type_of(_update)=type_of("")
   || SLO_KOD.cntx_psh();
      SLO_KOD.index('KOD');
      SLO_KOD.prefix();
      _ret:=
         _update(_typ,'NIESP','osoba niebędąca członkiem rodziny','T') &
         _update(_typ,'SYN','syn','T') &
         _update(_typ,'CORKA','córka','T') &
         _update(_typ,'MATKA','matka','T') &
         _update(_typ,'OJCIEC','ojciec','T') &
         _update(_typ,'MALZONEK','małżonek','T');
      {? _ret<>1
      || _ret:=-1
      ?};
      SLO_KOD.cntx_pop()
   || _ret:=-1
   ?}
|| _ret:=-1
?};
_txt:=
   'operacji zasilenia słownika: '
   ' * Oświadczenia wniosków o urlop opiekuńczy';
__UPG.msg('%1 %2'[{? _ret=1 || 'Powodzenie' || 'Niepowodzenie' ?},_txt]);

_ret


