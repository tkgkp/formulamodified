:!UTF-8
{TITLE:PIT-11 formularz obowiązujący w roku 2009}
{PRINTER: exec('prt','param_wydr','v',KST),,,,,'Q','C',,,,,,1,5,5,5,5,1,0}
{BEGIN:
   {? var_pres('pit')>100 || obj_del(pit) ?};
   pit:=obj_new(84);
   {! _n:=1..34 |! pit[_n]:='' !};
   {! _n:=35..obj_len(pit) |! pit[_n]:=0 !};

   exec('czytaj','#stalesys',date(__Rok,12,0),KST);

   pit[ 1]:=KST.NIP;
   pit[ 4]:='';
   pit[ 5]:='';
   pit[ 6]:='';
   pit[ 9]:=KST.NAZWA+', '+KST.REG;
   pit[10]:='POLSKA';
   pit[11]:=KST.WOJEWODZ;
   pit[12]:=KST.POWIAT;
   pit[13]:=KST.GMINA;
   pit[14]:=KST.ULICA;
   pit[15]:=KST.DOM;
   pit[16]:=KST.LOKAL;
   pit[17]:=KST.MIASTO;
   pit[18]:=KST.KOD;
   pit[19]:=KST.POCZTA;
   pit[82]:=VAT_DEK.PITR;
   pit[83]:=VAT_DEK.IMIE;
   pit[84]:=VAT_DEK.NAZ;
   {! _n:=8..19 |! pit[_n]:=~(-pit[_n]) !};

   czy_raport:=0;
   form_zg:="_x:=(_a*100)%100;_y:=((_a-((_a*100)%100))*100$0)%*100;
            {?_x=0&_y=0||form('zł,   gr',-25)||form(form(_x,16,0,'9,')+'zł, '+(('0'+$_y)+2)+'gr',-25)?}";

   CNT:="_c:=(_b-(+_a))%2; form(_c*' '+_a,_b)";
   {? VAT_DEK.KOR='T'
   || {? var_pres('__ord')>100 || obj_del(__ord) ?};
      __ord:=obj_new(13);
      {! _n:=1..obj_len(__ord) |! __ord[_n]:='' !};
      __ord[1]:=KST.NIP;
      __ord[5]:=KST.NAZWA;
      __ord[6]:=KST.SKROT;
      __ord[8]:=KST.REG
   ?}
}
{END:
   obj_del(pit); &pit;
   &CNT; &form_zg; &czy_raport;
   exec('stalesys','#stalesys');
   {? VAT_DEK.KOR='T' || obj_del(__ord); &__ord ?}
}
{COMMENT}OLD: dek1117.rpm{COMMENT-}
{COMMENT}OLD: dkw11171.rpi{COMMENT-}
{COMMENT}OLD: dkw11172.rpi{COMMENT-}
{COMMENT}OLD: ord_zu2.rpi{COMMENT-}
{
   czy_raport:=0;

   pit[4]:=VAT_DEK.OD$4;
   pit[5]:=VAT_DEK.DO$4;
   pit[6]:=VAT_DEK.US().NU+
              {? +US.UU || ', '+US.UU || '' ?}+
              {? +US.UK & +US.MU || ', '+(6+US.UK)+' '+US.MU || '' ?};
   pit[6]:=~-pit[6];
   pit[ 7]:=VAT_DEK.KOR;
   pit[20]:=VAT_DEK.NIP;
   pit[21]:=VAT_DEK.NAZWISKO;
   pit[22]:=VAT_DEK.PIERWSZE;
   pit[23]:=VAT_DEK.DATA_UR$4;
   pit[24]:=VAT_DEK.PESEL;
   pit[25]:='POLSKA';
   pit[26]:=VAT_DEK.WOJEWODZ;
   pit[27]:=VAT_DEK.POWIAT;
   pit[28]:=VAT_DEK.GMINA;
   pit[29]:=VAT_DEK.ULICA;
   pit[30]:=VAT_DEK.DOM;
   pit[31]:=VAT_DEK.LOKAL;
   pit[32]:=VAT_DEK.MIASTO;
   pit[33]:=VAT_DEK.KOD;
   pit[34]:=VAT_DEK.POCZTA;

   {! _n:=20..34 |! pit[_n]:=~-pit[_n] !};

   pit[35]:=#VAT_DEK.PKU;
   {? VAT_DEK.KOR='T'
   || __ord[2]:=VAT_DEK.NIP;
      __ord[9]:=VAT_DEK.NAZWISKO;
      __ord[10]:=VAT_DEK.PIERWSZE;
      __ord[11]:=VAT_DEK.DATA_UR$4;
      __ord[12]:=VAT_DEK.PESEL;
      __ord[13]:=exec('opis_korekty','edeklar');
      {! _n:=1..obj_len(__ord) |! __ord[_n]:=~(-__ord[_n]) !}
   ?};

   VAT_POZ.index('NR_NRK');
   VAT_POZ.prefix(VAT_DEK.ref);
   {? VAT_POZ.first
   || {!
      |?
      {! _ind:=1..4
      |! _kw:=($('VAT_POZ.KW'+$_ind))();
         _poz:=($('VAT_POZ.P'+$_ind))();
         {? _poz>=1 & _poz<=obj_len(pit)
         || pit[($('VAT_POZ.P'+$_ind))()]:=_kw
         ?};
         {? _ind=1 & VAT_POZ.NR<=10 || czy_raport+=_kw ?}
      !};
      VAT_POZ.next
      !}
   ?};
   ~~
}
