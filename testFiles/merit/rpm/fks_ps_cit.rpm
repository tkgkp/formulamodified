{MACRO: 'SRODEK'}
{ s[1]:=VAT_POZ.LP;
  s[2]:=form(VAT_POZ.OP,80);
  s[3]:=' '+VAT_POZ.TYP+' ';
  s[4]:={? VAT_POZ.TYP='T'
        || VAT_POZ.TEXT
        |? VAT_POZ.TYP='D'
        || 5*'  '+VAT_POZ.TEXT
        || form(form(VAT_POZ.NETTO,,2,' ,'),-30)
        ?};
  {? ver>=26
  || s[5]:={? VAT_POZ.TYP='T'
           || VAT_POZ.TEXT
           |? VAT_POZ.TYP='D'
           || 5*'  '+VAT_POZ.TEXT
           || form(form(VAT_POZ.VAT,,2,' ,'),-30)
           ?}
  ?};
~~}
