:!UTF-8
{TITLE:A. Nota dla controllingu}
{PRINTER: FINFO.W_PRN,,,,,'Q','C',,,,,,1,PAR_WYDR.MLEFT,PAR_WYDR.MTOP,
   PAR_WYDR.MRIGHT,PAR_WYDR.MBOT,0,1}
{BEGIN:
   PRN:=obj_new(@.CLASS.PRINT,8); PRN.sl_frame(); w:=obj_new(8);
   color:=prn1:=prn2:='PRN'; lm:=ll:=vp:=lmt:=lporz:=0; rsep:=PAR_WYDR.RSEP;
   head:=PAR_WYDR.HEAD; vp:=1; NOMAXP:=0
}
{END:
   VAR_DEL.delete('PRN','w','NOMAXP');
   &color; &prn1; &prn2; &lporz; &head;
   &lm; &ll; &vp; &lmt; &rsep
}
{MACRO: 'INIT'}
{  PRN.add("w[1]",20,'Okres',1);
   SKID_MBP.index('LP'); SKID_MBP.prefix(K__NOTY.SKID_MBN);
   {? SKID_MBP.first()
   || {!
      |? PRN.add(($('w['+$(SKID_MBP.LP+1)+']')),16,SKID_MBP.NAZ);
         SKID_MBP.next()
      !}
   ?};
   PRN.add("w[7]",16,'Wartość'@,1);
   PRN.add("w[8]",5,'Jedn.'@);
   ~~}
{MACRO-}
{MACRO: 'START'}
{  obj_del(PRN);
   PRN:=obj_new(@.CLASS.PRINT,8); PRN.sl_frame();~~}
   {SUBSTITUTE: 'INIT'}
{MACRO-}
{INCLUDE: wsp_pw.rpi}
{COMMENT}------------------------------SIGNATURE--------------------{COMMENT-}
{SUBSTITUTE: 'FOOT'}
{COMMENT}------------------------------MACRO WYDRUK-----------------{COMMENT-}
{MACRO: 'WYDRUK'}
{COMMENT}------------------------------REPORT-HEADER----------------{COMMENT-}
{PAR_WYDR.TITLE:='NOTA DLA CONTROLLINGU %1'@[K__NOTY.SYM];~~}
{SUBSTITUTE: 'HEAD'}
{FONT: 'T8G'}{SPACE: 'C'}
{ lmt:=int((charleft()-PRN.width())/2); ~~}
{<2 'Model: %1'@[K__NOTY.SKID_MBN().KOD]}
{<2 'Wersja: %1'@[K__NOTY.K_WERSJE().SYM]}
{<2 'Data noty: %1'@[$K__NOTY.DO]}{<34 'Data rejestracji: %1'@[$K__NOTY.DR]}{<66 'Zarejestrował: %1'@[K__NOTY.ZAR]}
{<2 'Opis dokumentu: %1'@[K__NOTY.OPIS]}
{FONT: 'T8GB'}
{SUBSTITUTE: 'TABHEAD'}{FONT: 'T8G'}{ vp:=1; ~~}
{COMMENT}------------------------------HEADER-----------------------{COMMENT-}
{HEADER}
{SUBSTITUTE: 'HEAD'}
{FONT: 'T8GB'}{SPACE: 'C'}
{SUBSTITUTE: 'TABHEAD'}
{HEADER-}
{COMMENT}------------------------------FOOTER-----------------------{COMMENT-}
{FOOTER: 0}
{SUBSTITUTE: 'LINIAF'}
{~~}
{FOOTER-}
{COMMENT}------------------------------WYDRUK-----------------------{COMMENT-}
{FONT: 'T8G'}{SPACE: 'C'}
{ rsep:=PAR_WYDR.RSEP; prn1:=prn2:='PRN';~~}
{FOR: 'K__PNOTY'}
{INDEX: 'K__NOTY'}
{PREFIX: K__NOTY.ref()}
{DO}
  { lporz+=1;
    w[1]:=K__PNOTY.OKRO_F().NAZ;
    w[2]:=K__PNOTY.WYM1().SYMBOL;
    w[3]:=K__PNOTY.WYM2().SYMBOL;
    w[4]:=K__PNOTY.WYM3().SYMBOL;
    w[5]:=K__PNOTY.WYM4().SYMBOL;
    w[6]:=K__PNOTY.WYM5().SYMBOL;
    w[7]:=form(K__PNOTY.WART,,4,' ,');
    w[8]:=K__PNOTY.JM().KOD;~~}
   {SUBSTITUTE: 'LINIA0'}
{OD}
{MACRO-}
{COMMENT}--------------------------END MACRO WYDRUK-----------------{COMMENT-}
{IF: var_pres('__K_NOTY')>100}
   {NOMAXP:=1;~~}
   {FOR: '__K_NOTY'}
   {DO}
      {IF: K__NOTY.seek(__K_NOTY.REF,)}
         {SUBSTITUTE: 'START'}
         {SUBSTITUTE: 'WYDRUK'}{l_druk+=1;~~}
         {IF: __K_NOTY.next()}{HEADER}{HEADER-}{PAGE}{page(1);~~}{FI}
      {FI}
   {OD}
   {czy_druk:=1;~~}
{ELSE}
   {SUBSTITUTE: 'INIT'}
   {SUBSTITUTE: 'WYDRUK'}
{FI}
