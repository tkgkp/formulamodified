{TITLE:F.  Deklaracja VAT-7}
{PRINTER: 'port_std',,,,,,,,,,,,1,17,15,10,,,1}
{BEGIN:
   pv:=obj_new(65); l:=i:=-1; STR.split(XINFO.NAZ+', '+XINFO.REGON); {! |? l+=1; STR.line(71); STR.next() !};
   {! _i:=1..65 |! pv[_i]:='' !};
   kl:='0:0:0,240:240:240'; bl:='0:0:0,255:255:255'; ks:='0:0:0,200:200:200';
   _size:=exec('vat_zd_size','fks_ved','N');
   vatzdstr:=_size%8+(_size%*8>0);
   vatzdzob:=exec('vat_zd_size','fks_ved','z')
}
{END:
   obj_del(pv); &pv; &kl; &bl; &ks; &l; &i;
   VAR_DEL.delete('vatzdstr','vatzdzob')
}
