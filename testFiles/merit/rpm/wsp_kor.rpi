{MACRO: 'POZYCJA'}
{ llin:=lineleft(); lpoz:=0; lmax:=0;
  il_lin:={? __walh<>__walp & PARAM_W.WALH='T' || 5 || 4 ?};
  params_set(params_get());
  _f:='params_set(params_get()); '+prn1+'.ini_line()'; _v:=($(_f))();
  _f:=prn1+'.length()'; lmax:=($(_f))(); headfoot:={? lmax>(llin-il_lin-1) || 2 || 0 ?};
  prnDoPrze:=0;
~~}
{WHILE: lmax>0}
{DO}
   {IF: llin<=(il_lin+1)}
      {bpage:=page(); ~~}
