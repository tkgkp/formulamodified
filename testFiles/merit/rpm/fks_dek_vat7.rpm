   PAR_WYDR.MLEFT,PAR_WYDR.MTOP,PAR_WYDR.MRIGHT,PAR_WYDR.MBOT,,1}
{BEGIN:
   pv:=obj_new(58); l:=i:=-1; STR.split(XINFO.NAZ); {! |? l+=1; STR.line(71); STR.next() !};
   kl:='0:0:0,240:240:240'; bl:='0:0:0,255:255:255'; ks:='0:0:0,200:200:200'
}
{END:
   obj_del(pv); &l; &i;
