:!UTF-8
{TITLE:PIT-40(17) formularz obowiązujący w roku 2012}
{PRINTER: exec('prt','param_wydr','v',KST),,,,,'Q','C',,,,,,1,5,5,5,5,1,0}
{BEGIN:
   {? var_pres('pit')>100 || obj_del(pit) ?};
   pit:=obj_new(84);
   {! _n:=1..33 |! pit[_n]:='' !};
   {! _n:=34..obj_len(pit) |! pit[_n]:=0 !};
   pit[69]:=pit[78]:='';

   exec('czytaj','#stalesys',date(__Rok,12,0),KST);

   pit[1]:=KST.NIP;
   pit[4]:='';
   pit[5]:='';
   pit[6]:='';
   pit[8]:=KST.NAZWA+', '+KST.REG;
   pit[9]:='Polska';
   pit[10]:=KST.WOJEWODZ;
   pit[11]:=KST.POWIAT;
   pit[12]:=KST.GMINA;
   pit[13]:=KST.ULICA;
   pit[14]:=KST.DOM;
   pit[15]:=KST.LOKAL;
   pit[16]:=KST.MIASTO;
   pit[17]:=KST.KOD;
   pit[18]:=KST.POCZTA;
   {! _n:=8..18 |! pit[_n]:=~(-pit[_n]) !};

   czy_raport:=0;
   CNT:="_c:=(_b-(+_a))%2; form(_c*' '+_a,_b)";


   {? var_pres('__ord')>100 || obj_del(__ord) ?};
   __ord:=obj_new(13);
   {! _n:=1..obj_len(__ord) |! __ord[_n]:='' !};
   __ord[1]:=KST.NIP;
   __ord[5]:=KST.NAZWA;
   __ord[6]:=KST.SKROT;
   __ord[8]:=KST.REG;
   VAR_DEL.delete('__P_UPR');
   __P_UPR:=exec('dostepne_p','schemat','PPL',P_FILTER.F_ZATR().KOD,'*');
   __P_UPR.prefix()
}
{END:
   obj_del(pit); &pit;
   &CNT; &czy_raport;
   exec('stalesys','#stalesys');
   obj_del(__ord); &__ord;
   VAR_DEL.delete('__P_UPR')
}
{COMMENT}OLD: deg4017.rpm{COMMENT-}
{COMMENT}OLD: dkw40171.rpi{COMMENT-}
{COMMENT}OLD: dkw40172.rpi{COMMENT-}
{COMMENT}OLD: dkw40173.rpi{COMMENT-}
{COMMENT}OLD: ord_zu2.rpi{COMMENT-}
{FOR: VAT_DEK}
{INDEX: 'VAT_PITG'}
{PREFIX: PIT_GRUP.ref}
{DO}
{IF: __P_UPR.find_key($VAT_DEK.P)}
{  czy_raport:=0;
   pit[4]:=pit[5]:=pit[6]:='';
   {! _nx:=19..33 |! pit[_nx]:='' !};
   {! _nx:=34..obj_len(pit) |! pit[_nx]:=0 !};
   pit[69]:=pit[78]:='';
   VAT_POZ.index('NR_NRK');
   VAT_POZ.prefix(VAT_DEK.ref);
   {? VAT_POZ.first
   || {!
      |?
      {! _ind:=1..4
      |! _kw:=($('VAT_POZ.KW'+$_ind))();
         _poz:=($('VAT_POZ.P'+$_ind))();
         {? _poz>=1 & _poz<=obj_len(pit)
         || pit[($('VAT_POZ.P'+$_ind))()]:=_kw;
            {? VAT_POZ.NR=12 || pit[69]:=82+VAT_POZ.RODZ ?};
            {? VAT_POZ.NR=20 || pit[78]:=82+VAT_POZ.RODZ ?}
         ?};
         {? _ind=1 & VAT_POZ.NR<=8 || czy_raport+=_kw ?}
      !};
      VAT_POZ.next
      !}
   ?};~~
}
{IF: czy_raport>0}
{  pit[4]:=VAT_DEK.OKRES;
   pit[5]:=VAT_DEK.US().NU+{? +US.UU || ', '+US.UU || '' ?}+
           {? +US.UK & +US.MU || ', '+(6+US.UK)+' '+US.MU || '' ?};
   pit[5]:=~-pit[5];
   pit[6]:=VAT_DEK.KOR;
   pit[19]:={? VAT_DEK.PESEL='' || VAT_DEK.NIP || VAT_DEK.PESEL ?};
   pit[20]:=VAT_DEK.DATA_UR$4;
   pit[21]:=VAT_DEK.NAZWISKO;
   pit[22]:=VAT_DEK.PIERWSZE;
   {? VAT_DEK.KRAJ<>null || pit[23]:=VAT_DEK.KRAJ().NAZ || pit[23]:='' ?};
   pit[24]:=VAT_DEK.WOJEWODZ;
   pit[25]:=VAT_DEK.POWIAT;
   pit[26]:=VAT_DEK.GMINA;
   pit[27]:=VAT_DEK.ULICA;
   pit[28]:=VAT_DEK.DOM;
   pit[29]:=VAT_DEK.LOKAL;
   pit[30]:=VAT_DEK.MIASTO;
   pit[31]:=VAT_DEK.KOD;
   pit[32]:=VAT_DEK.POCZTA;
   {! _n:=21..32 |! pit[_n]:=~(-pit[_n]) !};
   pit[33]:=VAT_DEK.PITR;
   pit[34]:=#VAT_DEK.PKU;
   pit[83]:=VAT_DEK.IMIE;
   pit[84]:=VAT_DEK.NAZ;
   {? VAT_DEK.KOR='T'
   || __ord[2]:=VAT_DEK.NIP;
      __ord[9]:=VAT_DEK.NAZWISKO;
      __ord[10]:=VAT_DEK.PIERWSZE;
      __ord[11]:=VAT_DEK.DATA_UR$4;
      __ord[12]:=VAT_DEK.PESEL;
      __ord[13]:=exec('opis_korekty','edeklar');
      {! _n:=1..obj_len(__ord) |! __ord[_n]:=~(-__ord[_n]) !}
   ?};
   ~~
}
