{REP:
   _ret:='{FONT: \'W10\'}{SPACE: \'B\'}';

   ZZ_GOT.cntx_psh();
   ZZ_GOT.index('MM');
   ZZ_GOT.prefix();
   _rodzic:=null;


   {! _aa:=1 .. 2
   |!
      frek:="
         _rodzic:=_a;
         _poziom:=_b;
         _lppref:=_c;
         _lpnr:=_d;
         _ret:=_e;

         _nl:='{LINE}';

         _ind:=_poziom*3;

         _lppref:=_lppref+{? _lpnr || $_lpnr+'. ' || '' ?};
         {? _lppref<>'' || _ret+='{ \\\'─\\\'*charleft()}'+_nl ?};
         _ret+='{<'+$_ind+'}'+_lppref+'Stanowisko:'+'{ \\\'.\\\'*charleft()}'+_nl;

         {? ZZ_GOT.first()
         || {!
            |?
               _ret+=
                  '{<8 '+znak+'} '+
                  exec('string2rep','phr_druk',
                     ZZ_GOT.OPIS+', minimalna '+$ZZ_GOT.MIN+', maksymalna '+$ZZ_GOT.MAX,
                     _ind,,1
                  );
               ZZ_GOT.next()
            !}
         ?};

         _ret+=__OO(3,'Komentarz:');
         _ret+='{FONT: \'\'W10\'\'}{SPACE: \'\'B\'\'}';
         _ret+=2*_nl;
         _ret
      ";

      _ret:=frek(_rodzic,0,'',0,_ret);
      &frek

   !};
   _ret+='{FONT: \'W10\'}{SPACE: \'B\'}';
   ZZ_GOT.cntx_pop();
   _ret
}