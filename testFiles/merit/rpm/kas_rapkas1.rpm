        PRN1:=obj_new(@.CLASS.PRINT,6); PRN1.sl_frame();
        PRN2:=obj_new(@.CLASS.PRINT,8); PRN2.sl_frame();
        PRN3:=obj_new(@.CLASS.PRINT,5); PRN3.sl_frame();
        color:=''; prn1:=prn2:='PRN'; lm:=ll:=lmt:=rsep:=0; vp:=1; anul:=0;
        separ:=separf:=r_wal:=licznik:=sm:=sw:=sm1:=sw1:=sp:=sp1:=separat:=0; do_przen:=1; r_macr:=0
}
{END: exec('done','dok_fks_aut_dok');
