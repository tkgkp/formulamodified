{TITLE: Raport kasowy}
{PRINTER: KINFO.W_PRN,,,,,'Q','C',,,,,,1,PAR_WYDR.MLEFT,PAR_WYDR.MTOP,PAR_WYDR.MRIGHT,PAR_WYDR.MBOT,0,1}
{BEGIN: exec('initWARL','dok_fks_aut_dok',USRCONST.ROK,'KASA','S');
