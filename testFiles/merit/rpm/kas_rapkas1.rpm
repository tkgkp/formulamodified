        EKSG.REF_ODD:=USRCONST.STANKAS().STANKAS().ODD;
        EKSG.STANKAS:=USRCONST.STANKAS;
        exec('set_fld','dok_fks_aut_dok','O','S'); exec('set_fld','dok_fks_aut_dok','N','S');
