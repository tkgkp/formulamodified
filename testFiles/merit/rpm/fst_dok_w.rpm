:!UTF-8
{TITLE:Wydruk dokumentu wartościowego}
{PRINTER: FINFO.W_PRN,,,,,'M','B',,,,,,1,
          PAR_WYDR.MLEFT,PAR_WYDR.MTOP,PAR_WYDR.MRIGHT,PAR_WYDR.MBOT,0,1}
{BEGIN:
   VAR_DEL.delete('czy_szablon');
   czy_szablon:=exec('rep_set','rap_zew','FST_DOKEW',1)='T';
   PRN:=obj_new(@.CLASS.PRINT,15); PRN.s_frame();
   c:=obj_new(15);
   {! _i:=1..14 |! c[_i] := '' !};
   i:=0;
   color:=prn1:=prn2:='';
   lm:=ll:=vp:=lmt:=rsep:=0;
   par1:=0; par2:=0; par3:=0; par4:=0; par5:=0;
   VAR_DEL.delete('__SRDO');
   __SRDO:=tab_tmp(1,'LP','INTEGER','Lp',
                     'CECHA','STRING[25]','Cecha',
                     'WARTOSC','STRING[255]','Wartość');
   __SRDO.LP:=1; __SRDO.CECHA:='Dla środka';
   __SRDO.WARTOSC:='['+SRDO.SRSR().NRI+'] '+SRDO.SRSR().NST;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Typ dokumentu'; __SRDO.WARTOSC:=SRDO.TYP().NAZWA;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Jedn. księg. dokumentu'; __SRDO.WARTOSC:=SRDO.ODD_DOK().OD;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Rodzaj dokumentu';
   __SRDO.WARTOSC:={? SRDO.P='+' || 'Przychodowy' |? SRDO.P='-' || 'Rozchodowy' || 'Inny' ?};
   __SRDO.add();
   {? SRDO.PRZYCHOD<>null
   || __SRDO.LP+=1; __SRDO.CECHA:='Rodzaj przychodu'; __SRDO.WARTOSC:=SRDO.PRZYCHOD().R;
      __SRDO.add()
   ?};
   {? SRDO.ROZ<>null
   || __SRDO.LP+=1; __SRDO.CECHA:='Rodzaj rozchodu'; __SRDO.WARTOSC:=SRDO.ROZ().R;
      __SRDO.add()
   ?};
   __SRDO.LP+=1; __SRDO.CECHA:='Opis operacji';     __SRDO.WARTOSC:=SRDO.UWAGI;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Data wprowadzenia'; __SRDO.WARTOSC:=$SRDO.DW;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Data obowiązywania'; __SRDO.WARTOSC:=$SRDO.DO;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Obowiązuje od okresu'; __SRDO.WARTOSC:=SRDO.OKRO_F().NAZ+' '+SRDO.ROK_F().NAZ;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Komisja'; __SRDO.WARTOSC:=SRDO.KOMISJA;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Przewodniczący'; __SRDO.WARTOSC:=SRDO.PRZ_KOM;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Orzeczenie komisji'; __SRDO.WARTOSC:=SRDO.ORZ_KOM;
   __SRDO.add();
   {? SRDO.TYP().WARP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Wartość podatkowa'; __SRDO.WARTOSC:=form(SRDO.WARP,,2);
      __SRDO.add()
   ?};
   {? SRDO.TYP().WARF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Wartość bilansowa'; __SRDO.WARTOSC:=form(SRDO.WARF,,2);
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().WARP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Wartość dodatkowa'; __SRDO.WARTOSC:=form(SRDO.WARD,,2);
      __SRDO.add()
   ?};
   {? SRDO.TYP().UMOP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Umorzenie podatkowe'; __SRDO.WARTOSC:=form(SRDO.UMOP,,2);
      __SRDO.add()
   ?};
   {? SRDO.TYP().UMOF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Umorzenie bilansowe'; __SRDO.WARTOSC:=form(SRDO.UMOF,,2);
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().UMOD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Umorzenie dodatkowe'; __SRDO.WARTOSC:=form(SRDO.UMOD,,2);
      __SRDO.add()
   ?};
   {? SRDO.TYP().OSTATEK='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Wartość rezydualna'; __SRDO.WARTOSC:=form(SRDO.OSTATEK,,2);
      __SRDO.add()
   ?};
   {? SRDO.TYP().WP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Współczynnik przeszacowania'; __SRDO.WARTOSC:=form(SRDO.WP,,2);
      __SRDO.add()
   ?};
   __SRDO.LP+=1; __SRDO.CECHA:='Dokument skreślający';
   __SRDO.WARTOSC:={? SRDO.Z='T' || 'Tak' || 'Nie' ?};
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Dokument zaksięgowany';
   __SRDO.WARTOSC:={? SRDO.K='T' || 'Tak' || 'Nie' ?};
   __SRDO.add();
   {? FINFO.SPR_GRP='T' & SRDO.TYP().P='S'
   || {? SRDO.TYP().WART_ZB='T'
      || __SRDO.LP+=1; __SRDO.CECHA:='Wartość zbycia podatkowa';
         __SRDO.WARTOSC:=form(SRDO.WARSP_P,,2);
         __SRDO.add();
         __SRDO.LP+=1; __SRDO.CECHA:='Wartość zbycia bilansowa';
         __SRDO.WARTOSC:=form(SRDO.WARSP_F,,2);
         __SRDO.add();
         {? FINFO.TOR_D='T'
         || __SRDO.LP+=1; __SRDO.CECHA:='Wartość zbycia dodatkowa';
            __SRDO.WARTOSC:=form(SRDO.WARSP_D,,2);
            __SRDO.add()
         ?};
         __SRDO.LP+=1; __SRDO.CECHA:='Firma docelowa';
         __SRDO.WARTOSC:=SRDO.FIRMA_D().SYMBOL+' - '+SRDO.FIRMA_D().OPIS;
         __SRDO.add()
      ?}
   ?};
   1
}
{END:
   obj_del(PRN);
   obj_del(c);
   &i; &color; &prn1; &prn2;
   &lm; &ll; &vp; &lmt; &rsep;
   &par1; &par2; &par3;&par4;&par5;
   VAR_DEL.delete('__SRDO','czy_szablon');
   1
}
{EXIT:
   exec('set_var','param_wydr');
   {? OPERATOR.DEPT<>null
      || PAR_WYDR.ODD:=OPERATOR.DEPT
      || PAR_WYDR.ODD:=0
   ?};
   0
}
{IF: czy_szablon}
   {exec('wypelnij','szablon','FST_DOKEW',
   {? SRDO.TYP().TYP='K+'
   || 'FST_DOKEW_KD'
   |? SRDO.TYP().TYP='K-'
   || 'FST_DOKEW_KU'
   |? SRDO.TYP().TYP='LT'
   || 'FST_DOKEW_LT'
   ?},,,);~~}
   {EXIT}
{FI}
