{FOR: ZF_WYK}{INDEX: 'OK'}{PREFIX: ZO_TEST.NP_DOK,ZO_TEST.ZZ_DOK,'T'}{DO}
{FOR: ZF_POZ}{INDEX: 'NUMER'}{PREFIX: ZF_WYK.ZF_DEF}{DO}
{IF: ZF_POZ.DOKUMENT='T'}
{  __rep:='';
   _name:=ZF_POZ.ZF_RPI().SZABLON;
   _acr:=ZF_POZ.ZF_SKL().KOD;
   _file:=fopen(_name,'ur',1,0);
   {? _file>0
   || __rep:={? _acr<>'DN' || __nag || '' ?};
      _line:='';
      {!
      |? (_line:=fread(_file))<>'\n'
      |! __rep+=_line
      !};
      fclose(_file)
   || FUN.emsg('Błąd dostępu do pliku %1.'@ [_name])
   ?};
   ~~
}
