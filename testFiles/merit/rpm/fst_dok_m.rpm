:!UTF-8
{TITLE:Wydruk dokumentu zmiany metody}
{PRINTER: FINFO.W_PRN,,,,,'M','B',,,,,,1,
          PAR_WYDR.MLEFT,PAR_WYDR.MTOP,PAR_WYDR.MRIGHT,PAR_WYDR.MBOT,0,1}
{BEGIN:
   VAR_DEL.delete('czy_szablon');
   czy_szablon:=exec('rep_set','rap_zew','FST_DOKEW',1)='T';
   PRN:=obj_new(@.CLASS.PRINT,15); PRN.s_frame();
   PRN1:=obj_new(@.CLASS.PRINT,1); PRN1.s_frame();
   c:=obj_new(15);
   {! _i:=1..14 |! c[_i] := '' !};
   i:=0;
   color:=prn1:=prn2:='';
   lm:=ll:=vp:=lmt:=rsep:=0; nagl:=0;
   par1:=0; par2:=0; par3:=0; par4:=0; par5:=0;
   VAR_DEL.delete('__SRDO');
   __SRDO:=tab_tmp(1,'LP','INTEGER','Lp',
                     'CECHA','STRING[25]','Cecha',
                     'WARTOSC','STRING[255]','Wartość',
                     'NAGL','INTEGER','Nagl');
   exec('set_srdo_pom','fst2');

   __SRDO.LP:=1; __SRDO.CECHA:='Dla środka';
   __SRDO.WARTOSC:='['+SRDO.SRSR().NRI+'] '+SRDO.SRSR().NST; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Typ dokumentu'; __SRDO.WARTOSC:=SRDO.TYP().NAZWA; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Jedn. księg. dokumentu'; __SRDO.WARTOSC:=SRDO.ODD_DOK().OD; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Data wprowadzenia'; __SRDO.WARTOSC:=$SRDO.DW; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Data obowiązywania'; __SRDO.WARTOSC:=$SRDO.DO; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Obowiązuje od okresu'; __SRDO.WARTOSC:=SRDO.OKRO_F().NAZ+' '+SRDO.ROK_F().NAZ; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Komisja'; __SRDO.WARTOSC:=SRDO.KOMISJA; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Przewodniczący'; __SRDO.WARTOSC:=SRDO.PRZ_KOM; __SRDO.NAGL:=0;
   __SRDO.add();
   __SRDO.LP+=1; __SRDO.CECHA:='Orzeczenie komisji'; __SRDO.WARTOSC:=SRDO.ORZ_KOM; __SRDO.NAGL:=0;
   __SRDO.add();

   __SRDO.LP+=1; __SRDO.CECHA:='Przed korektą'; __SRDO.WARTOSC:=''; __SRDO.NAGL:=1;
   __SRDO.add();
   {? SRDO.TYP().MP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Metoda podatkowa'; __SRDO.WARTOSC:=SRDO_POM.MP().K+' - '+SRDO_POM.MP().N; __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().MF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Metoda bilansowa'; __SRDO.WARTOSC:=SRDO_POM.MB().K+' - '+SRDO_POM.MB().N; __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().MD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Metoda dodatkowa'; __SRDO.WARTOSC:=SRDO_POM.MD().K+' - '+SRDO_POM.MD().N; __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().STAP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Stawka podatkowa'; __SRDO.WARTOSC:=form(SRDO_POM.STAP,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().STAF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Stawka bilansowa'; __SRDO.WARTOSC:=form(SRDO_POM.STAB,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().STAD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Stawka dodatkowa'; __SRDO.WARTOSC:=form(SRDO_POM.STAD,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().WKP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Korekta stawki podatkowej'; __SRDO.WARTOSC:=form(SRDO_POM.WKP,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().WKF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Korekta stawki bilansowej'; __SRDO.WARTOSC:=form(SRDO_POM.WKF,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().WKD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Korekta stawki dodatkowej'; __SRDO.WARTOSC:=form(SRDO_POM.WKD,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};

   __SRDO.LP+=1; __SRDO.CECHA:='Po korekcie'; __SRDO.WARTOSC:=''; __SRDO.NAGL:=1;
   __SRDO.add();
   {? SRDO.TYP().MP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Metoda podatkowa'; __SRDO.WARTOSC:=SRDO.MP().K+' - '+SRDO.MP().N; __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().MF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Metoda bilansowa'; __SRDO.WARTOSC:=SRDO.MF().K+' - '+SRDO.MF().N; __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().MD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Metoda dodatkowa'; __SRDO.WARTOSC:=SRDO.MD().K+' - '+SRDO.MD().N; __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().STAP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Stawka podatkowa'; __SRDO.WARTOSC:=form(SRDO.STAP,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().STAF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Stawka bilansowa'; __SRDO.WARTOSC:=form(SRDO.STAF,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().STAD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Stawka dodatkowa'; __SRDO.WARTOSC:=form(SRDO.STAD,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().WKP='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Korekta stawki podatkowej'; __SRDO.WARTOSC:=form(SRDO.WKP,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? SRDO.TYP().WKF='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Korekta stawki bilansowej'; __SRDO.WARTOSC:=form(SRDO.WKF,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   {? FINFO.TOR_D='T' & SRDO.TYP().WKD='T'
   || __SRDO.LP+=1; __SRDO.CECHA:='Korekta stawki dodatkowej'; __SRDO.WARTOSC:=form(SRDO.WKD,,2); __SRDO.NAGL:=0;
      __SRDO.add()
   ?};
   1
}
{END:
   obj_del(PRN);
   obj_del(c);
   obj_del(PRN1);
   &i; &color; &prn1; &prn2;
   &lm; &ll; &vp; &lmt; &rsep; &nagl;
   &par1; &par2; &par3;&par4;&par5;
   VAR_DEL.delete('__SRDO','czy_szablon');
   1
}
{EXIT:
   exec('set_var','param_wydr');
   {? OPERATOR.DEPT<>null
      || PAR_WYDR.ODD:=OPERATOR.DEPT
      || PAR_WYDR.ODD:=0
   ?};
   0
}
{IF: czy_szablon}
   {exec('wypelnij','szablon','FST_DOKEW','FST_DOKEW_MA',,,);~~}
   {EXIT}
{FI}
