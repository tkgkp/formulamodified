{BEGIN:
   VAR_DEL.delete('PRN1','PRN2','PRNS','PRNP','PRND','PRNX','NOMAXP','w','helper');
   PRN1:=obj_new(@.CLASS.PRINT,1); PRN1.s_frame();
   PRN2:=obj_new(@.CLASS.PRINT,2); PRN2.s_frame();

   PRNS:=obj_new(@.CLASS.PRINT,4); PRNS.s_frame();
   PRNP:=obj_new(@.CLASS.PRINT,2); PRNP.s_frame();
   PRND:=obj_new(@.CLASS.PRINT,4); PRND.s_frame();
   PRNX:=obj_new(@.CLASS.PRINT,3); PRNX.s_frame();

   w:=obj_new(4);
   color:=prn1:=prn2:='';
   lm:=ll:=vp:=lmt:=rsep:=NOMAXP:=0;
   helper:=obj_new('set','value');
   helper.set:="
      {! _ii:=1..obj_len(w) |! w[_ii]:='' !};
      {! _ii:=1.._ |! w[_ii]:=_[_ii] !};
      ~~
   ";
   helper.value:="
      _show:={? var_press('_b')>0 || _b || 1 ?};
      {? _show
      || form(_a,,{? eSpr.tys || 0 || 2 ?},' ,')
      || ''
      ?}
   ";
   ~~
}
{END:
   VAR_DEL.delete('PRN1','PRN2','PRNS','PRNP','PRND','PRNX','NOMAXP','w','helper','tabSpr','rsep2');
   &color; &prn1; &prn2;
   &lm; &ll; &vp; &vpp; &lmt; &rsep
}
