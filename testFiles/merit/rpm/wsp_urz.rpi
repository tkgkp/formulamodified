:!UTF-8
{
  kl:=3;
  kolu[1]:=10;
  kolu[2]:=60;
  kolu[3]:=60;
  il:=kolu[1]+kolu[2]+kolu[3]+kl+1;
  p[1]:=2+kolu[1];
   {!_a:=2 ..kl
   |! p[_a]:=1+p[_a-1]+kolu[_a]
   !};   ~~
}
{MACRO: 'LINIA1'}
┌{kolu[1]*'─'}┬{kolu[2]*'─'}┬{kolu[3]*'─'}┐
{MACRO-}
{MACRO: 'LINIA2'}
├{kolu[1]*'─'}┼{kolu[2]*'─'}┼{kolu[3]*'─'}┤
{MACRO-}
{MACRO: 'LINIA3'}
└{kolu[1]*'─'}┴{kolu[2]*'─'}┴{kolu[3]*'─'}┘
{MACRO-}
{COMMENT}------------------------------------------------------------------{COMMENT-}
{HEADER}
{PAR_WYDR.TITLE:='Słownik użytkownika - %1'@[SLU.NAZ];~~}{SUBSTITUTE: 'CHARHEAD'}{FONT: 'R'}
{LINE: 1}{SPACE: 'C'}
{SUBSTITUTE: 'LINIA1'}
{<1'│'}{<1>{p[1]}'Kod'@}{<{p[1]}'│'}{<{p[1]}>{p[2]}'Nazwa urzędu'@}{<{p[2]}'│'}{<{p[2]}>{p[2]}'Adres'}{<{p[3]}'│'}
{SUBSTITUTE: 'LINIA2'}
{HEADER-}
{COMMENT}------------------------------------------------------------------{COMMENT-}
{FOOTER:0}
{FONT: 'Q'}{SPACE: 'C'}{SUBSTITUTE: 'LINIA3'}
{SUBSTITUTE: 'CHARFOOT'}
{~~}
{FOOTER-}
{COMMENT}----------------------CIALO-WYDRUKU-------------------------------{COMMENT-}
{FONT: 'Q'}{SPACE: 'C'}
{FOR: 'SLO'}
{INDEX: 'SL'}
{PREFIX: SLU.ref}
{DO}
   {FOR: 'US'}
   {INDEX: 'URZSKNAZ'}
   {PREFIX: SLO.TR}
   {DO}
    { w[1]:=US.SYM;
      w[2]:=US.NU;
      w[3]:=US.MU;
      w44:=US.UU;   ~~}
    {ENTIRE}
    {<{1}'│'} {w[1]}{<{p[1]}'│'} {w[2]}{<{p[2]}'│'} {w[3]}{<{p[3]}'│'}
    {<{1}'│'}{<{p[1]}'│'}{<{p[2]}'│'} {w44}{<{p[3]}'│'}
    {ENTIRE-}
   {OD}
{OD}
{COMMENT}------------------------------------------------------------------{COMMENT-}
{FOOTER}
{SUBSTITUTE: 'CHARFOOT'}
{~~}
{FOOTER-}
