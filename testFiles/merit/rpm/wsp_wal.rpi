{FOOTER:0}
{FONT: 'Q'}{SPACE: 'C'}{SUBSTITUTE: 'LINIA3'}
{SUBSTITUTE: 'CHARFOOT'}
{~~}
{FOOTER-}
{COMMENT}------------------------CIALO-WYDRUKU-----------------------------{COMMENT-}
{FONT: 'Q'}{SPACE: 'C'}
{FOR: 'SLO'}
{INDEX: 'SL'}
{PREFIX: SLU.ref}
{DO}
   {FOR: 'WAL'}
   {INDEX: 'WAL_SYM'}
   {PREFIX: SLO.KOD}
   {DO}
       { w[1]:=WAL.KOD;
         w[2]:=WAL.SYM;
         w[3]:=WAL.NAZ;
         w[4]:=WAL.KRAJ;  ~~}
    {<{1}'│'} {w[1]}{<{p[1]}'│'} {w[2]}{<{p[2]}'│'} {w[3]}{<{p[3]}'│'} {w[4]}{<{p[4]}'│'}
   {OD}
{OD}
