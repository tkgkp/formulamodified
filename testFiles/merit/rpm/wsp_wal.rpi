{HEADER}
{PAR_WYDR.TITLE:='Słownik użytkownika - %1'@[SLU.NAZ];~~}{SUBSTITUTE: 'CHARHEAD'}{FONT: 'R'}
{LINE: 1}{SPACE: 'C'}
{SUBSTITUTE: 'LINIA1'}
{<1'│'}{<1>{p[1]}'Kod'@}{<{p[1]}'│'}{<{p[1]}>{p[2]}'Symbol'@}{<{p[2]}'│'}{<{p[2]}>{p[3]}'Nazwa waluty'@}{<{p[3]}'│'}{<{p[3]}>{p[4]}'Kraj waluty'@}{<{p[4]}'│'}
{SUBSTITUTE: 'LINIA2'}
{HEADER-}
