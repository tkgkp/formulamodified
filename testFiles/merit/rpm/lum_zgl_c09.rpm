{<{lmt+1}}{'Parametry wydruku:'@}
{<{lmt+1}}{'Data wykonania:'@}
{<{lmt+1}}{'od %1 do %2'@[$WYDRUKIL.OD_DATY,$WYDRUKIL.DO_DATY]}
{FONT: 'T8GB'}
