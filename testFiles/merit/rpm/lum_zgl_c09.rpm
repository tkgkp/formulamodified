{FOOTER: 0}
{SUBSTITUTE: 'LINIAF'}
{IF: tryb<>1}

{<{lmt+1}'Data i podpis przekazującego wykonanie'@}{>{lmt+192}'Data  i podpis przyjmującego wykonanie'@}

{<{lmt+1}'......................................'}{>{lmt+192}'......................................'}

{LINE: 1}
{FI}
{FOOTER-}
{COMMENT}------------------------------WYDRUK-----------------------{COMMENT-}
{FONT: 'T8G'}{SPACE: 'C'}
{ rsep:=PAR_WYDR.RSEP; w[1]:=0;
  dat_min:=WYDRUKIL.OD_DATY; dat_max:=WYDRUKIL.DO_DATY;
~~}
{FOR: 'OKR'}
{INDEX: 'OKR'}
{DO}
   {IF: (OKR.ROK >= dat_min~1 & OKR.MC >= dat_min~2) & (OKR.ROK <= dat_max~1 & OKR.MC <= dat_max~2)}
      {exec('zle_open','open_tab',ST.ODDZ,2-$OKR.ROK,form(OKR.MC,-2));~~}
      {FOR: 'ZLP'}
      {INDEX: 'R'}
      {PREFIX: 'N'}
      {DO}
      {IF: ((WYDRUKIL.OD_DATY<=ZLP.DW | WYDRUKIL.OD_DATY=date(0,0,0))
          & (WYDRUKIL.DO_DATY>=ZLP.DW | WYDRUKIL.DO_DATY=date(0,0,0)))
         & ((WYDRUKIL.RADIOB_1=0)
          | (WYDRUKIL.RADIOB_1=1 & ZLP.ZLE().UP<>null)
          | (WYDRUKIL.RADIOB_1=2 & ZLP.ZLE().UP=null))
         }
         {
          exec('zlp_umo','umowy_zlecenia');
          w[1]+=1;
          w[2]:={? ZLP.DP<>date(0,0,0) || $ZLP.DP || '' ?};
          w[3]:={? ZLP.DW<>date(0,0,0) || $ZLP.DW || '' ?};
          w[4]:=ZLP.ZLE().POS().UL().MIA().NAZ+' '+ZLP.ZLE().POS().UL().UL+' '+ZLP.ZLE().POS().NR;
          w[5]:=ZLP.IL;
          w[6]:=ZLP.MJ().J().KOD;
          w[7]:={? w[6]='m3' || ZLP.IL || UMO.IL2 ?};
          w[8]:=ZLP.C;
          w[9]:={? tryb<>1 || ZLP.ZLE().KH().KOD+' ' || '' ?}+ZLP.ZLE().KH().NAZ;
          kkh:=ZLP.ZLE().KH().KOD;
          w[10]:=ZLP.MJ().KTM;
          ~~}
         {SUBSTITUTE: 'LINIA0'}
         {FI}
      {OD}
   {FI}
{OD}
{PAGE}