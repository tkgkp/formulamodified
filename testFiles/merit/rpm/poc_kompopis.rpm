{REP:
   _nazwa:='';
   ZZ_KOMP.cntx_psh();
   _loop:=ZZ_KOMP.ZZ_KOMP;
   {!
   |? _loop
   |! {? ZZ_KOMP.seek(ZZ_KOMP.ZZ_KOMP,)
      || _nazwa+=ZZ_KOMP.NAZWA+', ';
         _loop:=ZZ_KOMP.ZZ_KOMP
      || _loop:=0
      ?}
   !};
   ZZ_KOMP.cntx_pop();
   {? _nazwa<>'' || _nazwa:='('+_nazwa-2+')' ?};

   exec('string2rep','phr_druk',_nazwa)
}
