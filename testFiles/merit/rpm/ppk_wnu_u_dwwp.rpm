{TITLE:Deklaracja uczestnika PPK w zakresie finansowania wpłat podstawowych}
{PRINTER: exec('prt','param_wydr','v',KST),,,,,'Q','C',,,,,,1,5,5,5,5,1,1,'A4'}
{BEGIN:
   VAR_DEL.delete('dane');
   KST_PPK.cntx_psh();
   exec('czytaj','#stalesys',PPK_WNU.DATA,KST_PPK);
   dane:=exec('obj_ntab_set','#array',exec('dane','ppk_wnu'),
      'SK_MINPU',KST_PPK.SK_MINPU,
      'SK_MAXPU',KST_PPK.SK_MAXPU,
      'PROC',0
   );
   KST_PPK.cntx_pop();
   PPK_WPW.cntx_psh();
   PPK_WPW.index('RN');
   PPK_WPW.prefix(PPK_WNU.ref(),__RUB.sys_kod(712,PPK_WNU.OD,1));
   {? PPK_WPW.first()
   || dane.PROC:=PPK_WPW.STAWKA
   ?};
   PPK_WPW.cntx_pop();
   ~~
}
{END:
      VAR_DEL.delete('dane');
      ~~
}
