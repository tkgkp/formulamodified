:!UTF-8
{TITLE:Potwierdzenie odbioru Odpadów}
{PRINTER: exec('set_parw','param_wydr');exec('prt','param_wydr','v'),,,,,'Q','C',,,,,,1,
   PAR_WYDR.MBOT,PAR_WYDR.MLEFT,PAR_WYDR.MTOP,PAR_WYDR.MRIGHT,0,1}
{BEGIN:

   _ilekol:=4;

   {? var_pres('PRN')>100  || obj_del(PRN)  ?}; PRN:=obj_new(@.CLASS.PRINT,_ilekol);  PRN.s_frame();

   {? var_pres('w')>100    || obj_del(w)    ?}; w:=obj_new(_ilekol);

   {! _i:=1..._ilekol |! w[_i]:='' !};

   'zmienne pomocnicze do wydrukow graficznych';
   color:='';
   prn1:='PRN'; prn2:='PRNS';
   lm:=ll:=vp:=lmt:=lp:=0; rsep:=PAR_WYDR.RSEP;
   PAR_WYDR.TITLE:='';
   GPODPOZ.cntx_psh();~~}
{END:
   VAR_DEL.delete('PRN','PRNS','w');
   GPODPOZ.cntx_pop();
   &color; &prn1; &prn2; &lp;
   &lm; &ll; &vp; &lmt; &rsep
;~~}
{INCLUDE: wsp_pw.rpi}
{INCLUDE: wsp_pw2.rpi}
{FOR:__xdr}
{DO}
{GPODPOZ.seek(__xdr.REF,);~~}
{FIRST}
