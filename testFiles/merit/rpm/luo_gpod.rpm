   ||
      _ll:=charleft()-5-15-25-10
   ?};
   {? _ll<=0  ||  _ll:=40 |? _ll>40 || _ll:=40 ?};
   PRN.add("w[3]",_ll,'Kod i Nazwa odpadu');
   PRN.add("w[4]",25,'Waga netto [Kg]',1);
   lp:=0;~~}
