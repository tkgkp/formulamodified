{IF: par[2]}
{FONT: 'T8'}{ h_rat:=charleft(); ~~ }
{FONT: 'T8G'}{ h_rat:=charleft()/{? par[5] || 1 || h_rat ?}; ~~ }
   {SPACE: 'C'}
   {LINE}
   { par[2]:=0; ~~ }
   {<{ceil(lmt*h_rat)+1}'Parametry sprawozdania'@}
   {<{ceil(lmt*h_rat)+1}'Data wznowienia: %1'[VAR_EDIT.DATA$1]}
{FI}

{FONT: 'T8B'}
{SPACE: 'B'}
{LINE}
{SUBSTITUTE: 'TABHEAD'}
